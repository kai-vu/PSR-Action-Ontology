@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix psr: <https://w3id.org/psr-action#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix ex: <http://example.org/data/> .

ex:kitchen a psr:Environment ;
    dul:hasComponent ex:counter, ex:refrigerator, ex:sink, ex:robot, ex:diningTable, ex:coffeeMachine, ex:cups, ex:bottle .

ex:counter a psr:Component ;
    dul:hasLocation ex:kitchenLocation ;
    psr:hasAffordance soma:Grasping, soma:Holding .

ex:refrigerator a psr:Component ;
    dul:hasLocation ex:kitchenLocation ;
    psr:hasAffordance soma:Opening, soma:Closing .

ex:sink a psr:Component ;
    dul:hasLocation ex:kitchenLocation ;
    psr:hasAffordance soma:Grasping .

ex:robot a psr:Component ;
    dul:hasLocation ex:diningTableLocation ;
    psr:hasAffordance soma:Grasping, soma:Holding .

ex:diningTable a psr:Component ;
    dul:hasLocation ex:diningAreaLocation ;
    psr:hasAffordance soma:Grasping, soma:Holding .

ex:coffeeMachine a psr:Component ;
    dul:hasLocation ex:counterLocation ;
    psr:hasAffordance soma:Grasping, soma:Holding .

ex:cups a psr:Component ;
    dul:hasLocation ex:counterLocation ;
    psr:hasAffordance soma:Grasping, soma:Holding .

ex:bottle a psr:Component ;
    dul:hasLocation ex:diningTableLocation ;
    psr:hasAffordance soma:Grasping, soma:Holding .

ex:kitchenLocation a soma:Location ;
    geo:sfWithin ex:kitchen .

ex:counterLocation a soma:Location ;
    geo:sfWithin ex:counter ;
    psr:onTopOf ex:counter .

ex:refrigeratorLocation a soma:Location ;
    geo:sfWithin ex:refrigerator ;
    psr:onTopOf ex:refrigerator .

ex:sinkLocation a soma:Location ;
    geo:sfWithin ex:sink ;
    psr:onTopOf ex:sink .

ex:diningTableLocation a soma:Location ;
    geo:sfWithin ex:diningTable ;
    psr:onTopOf ex:diningTable .

ex:diningAreaLocation a soma:Location ;
    geo:sfWithin ex:diningArea ;
    psr:onTopOf ex:diningArea .

ex:diningArea a psr:Environment ;
    dul:hasComponent ex:diningTable, ex:bottle, ex:doorway ;
    dul:hasLocation ex:diningAreaLocation .

ex:doorway a psr:Component ;
    dul:hasLocation ex:diningAreaLocation ;
    psr:hasAffordance soma:Grasping, soma:Holding .