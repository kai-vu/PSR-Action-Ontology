@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix geo:    <http://www.opengis.net/ont/geosparql#> .
@prefix soma:   <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul:    <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr:    <https://w3id.org/psr-task#> .

:Kitchen a psr:Environment .

:BlackCabinet1 a psr:Furniture, psr:Component ;
    dul:hasLocation :KitchenLocation1 .

:KitchenLocation1 a psr:Location .

:Countertop a psr:Component, psr:Furniture ;
    dul:hasLocation :KitchenLocation2 .

:KitchenLocation2 a psr:Location .

:Refrigerator a psr:Appliance, psr:Component ;
    dul:hasLocation :KitchenLocation3 ;
    psr:hasAffordance :Closing ;
    psr:hasAffordance :Opening .

:KitchenLocation3 a psr:Location .

:Sinks a psr:Appliance, psr:Component ;
    dul:hasLocation :KitchenLocation4 .

:KitchenLocation4 a psr:Location .

:Table a psr:Furniture, psr:Component ;
    dul:hasLocation :KitchenLocation5 .

:KitchenLocation5 a psr:Location .

:Chair1 a psr:Furniture, psr:Component ;
    dul:hasLocation :KitchenLocation6 .

:KitchenLocation6 a psr:Location .

:Chair2 a psr:Furniture, psr:Component ;
    dul:hasLocation :KitchenLocation7 .

:KitchenLocation7 a psr:Location .

:Chair3 a psr:Furniture, psr:Component ;
    dul:hasLocation :KitchenLocation8 .

:KitchenLocation8 a psr:Location .

:Chair4 a psr:Furniture, psr:Component ;
    dul:hasLocation :KitchenLocation9 .

:KitchenLocation9 a psr:Location .

:HouseModel a psr:Object, psr:Component ;
    dul:hasLocation :TableLocation .

:TableLocation a psr:Location ;
    geo:sfOnTopOf :Table .

:CupcakeHolder a psr:Object, psr:Component ;
    dul:hasLocation :TableLocation2 .

:TableLocation2 a psr:Location ;
    geo:sfOnTopOf :Table .

:Picture a psr:Object, psr:Component ;
    dul:hasLocation :WallLocation1 .

:WallLocation1 a psr:Location .

:Stove a psr:Appliance, psr:Component ;
    dul:hasLocation :KitchenLocation10 .

:KitchenLocation10 a psr:Location .

:Shelf a psr:Furniture, psr:Component ;
    dul:hasLocation :KitchenLocation11 .

:KitchenLocation11 a psr:Location .

:CabinetSet a psr:Furniture, psr:Component ;
    dul:hasLocation :KitchenLocation12 .

:KitchenLocation12 a psr:Location .

:CerealBox a psr:Object, psr:Component ;
    dul:hasLocation :CountertopLocation .

:CountertopLocation a psr:Location ;
    geo:sfOnTopOf :Countertop .

:Cup1 a psr:Object, psr:Component ;
    dul:hasLocation :CountertopLocation2 .

:CountertopLocation2 a psr:Location ;
    geo:sfOnTopOf :Countertop .

:Cup2 a psr:Object, psr:Component ;
    dul:hasLocation :CountertopLocation3 .

:CountertopLocation3 a psr:Location ;
    geo:sfOnTopOf :Countertop .

:Bottle1 a psr:Object, psr:Component ;
    dul:hasLocation :CountertopLocation4 .

:CountertopLocation4 a psr:Location ;
    geo:sfOnTopOf :Countertop .

:Bottle2 a psr:Object, psr:Component ;
    dul:hasLocation :CountertopLocation5 .

:CountertopLocation5 a psr:Location ;
    geo:sfOnTopOf :Countertop .

:GlassJar a psr:Object, psr:Component ;
    dul:hasLocation :TableLocation3 .

:TableLocation3 a psr:Location ;
    geo:sfOnTopOf :Table .

:Mug a psr:Object, psr:Component ;
    dul:hasLocation :TableLocation4 .

:TableLocation4 a psr:Location ;
    geo:sfOnTopOf :Table .

:Kitchen a psr:Environment ;
    dul:hasComponent :BlackCabinet1 ;
    dul:hasComponent :Countertop ;
    dul:hasComponent :Refrigerator ;
    dul:hasComponent :Sinks ;
    dul:hasComponent :Table ;
    dul:hasComponent :Chair1 ;
    dul:hasComponent :Chair2 ;
    dul:hasComponent :Chair3 ;
    dul:hasComponent :Chair4 ;
    dul:hasComponent :HouseModel ;
    dul:hasComponent :CupcakeHolder ;
    dul:hasComponent :Picture ;
    dul:hasComponent :Stove ;
    dul:hasComponent :Shelf ;
    dul:hasComponent :CabinetSet ;
    dul:hasComponent :CerealBox ;
    dul:hasComponent :Cup1 ;
    dul:hasComponent :Cup2 ;
    dul:hasComponent :Bottle1 ;
    dul:hasComponent :Bottle2 ;
    dul:hasComponent :GlassJar ;
    dul:hasComponent :Mug .