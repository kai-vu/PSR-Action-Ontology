@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix psr: <https://w3id.org/psr-task#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .

:Kitchen a psr:Environment .
:Kitchen dul:hasComponent :Refrigerator .
:Kitchen dul:hasComponent :LightFixture1 .
:Kitchen dul:hasComponent :Shelf1 .
:Kitchen dul:hasComponent :OrangeContainer .
:Kitchen dul:hasComponent :FoodBoxes .
:Kitchen dul:hasComponent :Sink .
:Kitchen dul:hasComponent :CuttingBoard .
:Kitchen dul:hasComponent :DishDrainer .
:Kitchen dul:hasComponent :WhiteChair .
:Kitchen dul:hasComponent :Cabinet1 .
:Kitchen dul:hasComponent :Countertop .

:DiningArea a psr:Environment .
:DiningArea dul:hasComponent :DiningTable .
:DiningArea dul:hasComponent :BrownSofa .
:DiningArea dul:hasComponent :LightFixture2 .
:DiningArea dul:hasComponent :PictureFrame .
:DiningArea dul:hasComponent :Shelf2 .
:DiningArea dul:hasComponent :WhiteDiningChair .
:DiningArea dul:hasComponent :Cabinet2 .

:Refrigerator a psr:Appliance .
:Refrigerator dul:hasLocation :RefrigeratorLocation .
:RefrigeratorLocation geo:sfOverlaps :WhiteSurface .

:LightFixture1 a psr:Component .
:LightFixture1 dul:hasLocation :LightFixture1Location .
:LightFixture1Location geo:sfOverlaps :RefrigeratorLocation .

:Shelf1 a psr:Component .
:Shelf1 dul:hasLocation :Shelf1Location .
:Shelf1Location geo:sfWithin :Kitchen .

:OrangeContainer a psr:Object .
:OrangeContainer dul:hasLocation :Shelf1Location .

:FoodBoxes a psr:Object .
:FoodBoxes dul:hasLocation :Shelf1Location .

:Sink a psr:Component .
:Sink dul:hasLocation :SinkLocation .
:SinkLocation geo:sfWithin :Kitchen .

:CuttingBoard a psr:Component .
:CuttingBoard dul:hasLocation :CuttingBoardLocation .
:CuttingBoardLocation geo:sfOverlaps :SinkLocation .

:DishDrainer a psr:Component .
:DishDrainer dul:hasLocation :DishDrainerLocation .
:DishDrainerLocation geo:sfWithin :Kitchen .

:WhiteChair a psr:Component .
:WhiteChair dul:hasLocation :WhiteChairLocation .
:WhiteChairLocation geo:sfWithin :Kitchen .

:Cabinet1 a psr:Component .
:Cabinet1 dul:hasLocation :Cabinet1Location .
:Cabinet1Location geo:sfWithin :Kitchen .

:Countertop a psr:Component .
:Countertop dul:hasLocation :CountertopLocation .
:CountertopLocation geo:sfWithin :Kitchen .

:DiningTable a psr:Component .
:DiningTable dul:hasLocation :DiningTableLocation .
:DiningTableLocation geo:sfWithin :DiningArea .

:BrownSofa a psr:Component .
:BrownSofa dul:hasLocation :BrownSofaLocation .
:BrownSofaLocation geo:sfWithin :DiningArea .

:LightFixture2 a psr:Component .
:LightFixture2 dul:hasLocation :LightFixture2Location .
:LightFixture2Location geo:sfOverlaps :DiningTableLocation .

:PictureFrame a psr:Component .
:PictureFrame dul:hasLocation :PictureFrameLocation .
:PictureFrameLocation geo:sfWithin :DiningArea .

:Shelf2 a psr:Component .
:Shelf2 dul:hasLocation :Shelf2Location .
:Shelf2Location geo:sfWithin :DiningArea .

:WhiteDiningChair a psr:Component .
:WhiteDiningChair dul:hasLocation :WhiteDiningChairLocation .
:WhiteDiningChairLocation geo:sfWithin :DiningArea .

:Cabinet2 a psr:Component .
:Cabinet2 dul:hasLocation :Cabinet2Location .
:Cabinet2Location geo:sfWithin :DiningArea .