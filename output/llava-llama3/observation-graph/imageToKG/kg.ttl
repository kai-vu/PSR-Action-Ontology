@prefix rdf: <http://www.w3.org/1999/01/rdf-schema#> .
@prefix dul: <http://example.com/ontology#> .
@prefix geo: <http://www.opengis.net/def/201703/wfs.md#> .

# Environment
:Environment a dul:Entity ;
    dul:hasComponent :Appliance ;
    dul:hasComponent :Furniture ;
    dul:hasComponent :Object ;
    geo:sfOverlaps :Appliance ;
    geo:sfOverlaps :Furniture ;
    geo:sfContains :Furniture ;
    geo:sfWithin :Object .

# Appliance
:Appliance a dul:Entity ;
    dul:hasAffordance :Closing ;
    dul:hasAffordance :Opening ;
    dul:hasAffordance :Delivering ;
    dul:hasLocation :StandardLocation.

# Furniture
:Furniture a dul:Entity ;
    dul:hasLocation :CurrentLocation ;
    dul:hasAffordance :Closing ;
    dul:hasAffordance :Opening ;
    dul:hasAffordance :Delivering ;
    dul:hasAffordance :PickingUp ;
    dul:hasAffordance :PuttingDown ;
    dul:hasAffordance :Pulling ;
    dul:hasAffordance :Grabbing .

# Object
:Object a dul:Entity ;
    dul:hasLocation :CurrentLocation.

# Standard Location
:StandardLocation a dul:Entity ;
    geo:sfContains :Furniture.