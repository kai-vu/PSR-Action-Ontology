@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix psr: <http://www.ease-crc.org/ont/psr/0.1#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf#> .
@prefix dul: <http://www.ease-crc.org/ont/DUL#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA#> .
@prefix owl: <http://www.w3.org/2002/12/owl#> .

<http://example.com/environment_01> a psr:Environment, rdfs:Resource.
<http://example.com/component_01> a psr:Component, dul:Entity, geo:SpatialThing ;
    psr:hasAffordance <http://example.com/affordance_open>;
    psr:hasLocation <http://example.com/location_01>.
<http://example.com/component_02> a psr:Component, dul:Entity, geo:SpatialThing ;
    psr:hasAffordance <http://example.com/affordance_close>;
    psr:hasLocation <http://example.com/location_01>.
<http://example.com/component_03> a psr:Component, dul:Entity, geo:SpatialThing ;
    psr:hasAffordance <http://example.com/affordance_push>;
    psr:hasLocation <http://example.com/location_02>.
<http://example.com/location_01> a geo:SpatialThing, dul:Entity.
<http://example.com/location_02> a geo:SpatialThing, dul:Entity.
  
<http://example.com/affordance_open> a psr:Affordance ;
  rdfs:label "Open"@en.
  
<http://example.com/affordance_close> a psr:Affordance ;
  rdfs:label "Close"@en.
  
<http://example.com/affordance_push> a psr:Affordance ;
  rdfs:label "Push"@en.