@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix geo:    <http://www.opengis.net/ont/geosparql#> .
@prefix soma:   <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul:    <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr:    <https://w3id.org/psr-action#> .
@prefix ex:     <http://example.org/data/> .

ex:kitchen a psr:Environment .
ex:kitchen dul:hasComponent ex:counter .
ex:kitchen dul:hasComponent ex:refrigerator .
ex:kitchen dul:hasComponent ex:sink .
ex:kitchen dul:hasComponent ex:robot .
ex:kitchen dul:hasComponent ex:diningTable .

ex:counter a psr:Component .
ex:counter dul:hasLocation ex:counterLocation .
ex:counter psr:hasAffordance soma:Pulling .
ex:counter psr:hasAffordance soma:Pushing .

ex:counterLocation a psr:Location .
ex:counterLocation geo:sfWithin ex:kitchen .

ex:refrigerator a psr:Appliance .
ex:refrigerator dul:hasLocation ex:refrigeratorLocation .
ex:refrigerator psr:hasAffordance soma:Opening .
ex:refrigerator psr:hasAffordance soma:Closing .

ex:refrigeratorLocation a psr:Location .
ex:refrigeratorLocation geo:sfWithin ex:kitchen .

ex:sink a psr:Component .
ex:sink dul:hasLocation ex:sinkLocation .

ex:sinkLocation a psr:Location .
ex:sinkLocation geo:sfWithin ex:kitchen .

ex:robot a psr:Object .
ex:robot dul:hasLocation ex:robotLocation .
ex:robot psr:hasAffordance soma:Grasping .
ex:robot psr:hasAffordance soma:Holding .

ex:robotLocation a psr:Location .
ex:robotLocation psr:onTopOf ex:table .

ex:table a psr:Furniture .
ex:table dul:hasLocation ex:tableLocation .

ex:tableLocation a psr:Location .
ex:tableLocation geo:sfWithin ex:kitchen .

ex:diningTable a psr:Furniture .
ex:diningTable dul:hasLocation ex:diningTableLocation .

ex:diningTableLocation a psr:Location .
ex:diningTableLocation geo:sfWithin ex:diningArea .

ex:diningArea a psr:Environment .
ex:diningArea dul:hasComponent ex:diningTable .
ex:diningArea dul:hasComponent ex:door .

ex:door a psr:Component .
ex:door dul:hasLocation ex:doorLocation .
ex:door psr:hasAffordance soma:Opening .
ex:door psr:hasAffordance soma:Closing .

ex:doorLocation a psr:Location .
ex:doorLocation geo:sfWithin ex:diningArea .