@prefix kitchen: <http://example.org/kitchen#> .

kitchen:kitchen kitchen:hasComponent kitchen:cabinetry,
        kitchen:counter,
        kitchen:countertop,
        kitchen:refrigerator,
        kitchen:robot,
        kitchen:sink ;
    kitchen:sfAdjacent kitchen:dining_area .

kitchen:counter kitchen:hasComponent kitchen:coffee_machine,
        kitchen:cups .

kitchen:dining_area kitchen:hasComponent kitchen:dining_table,
        kitchen:doorway,
        kitchen:glass_door .

kitchen:dining_table kitchen:hasComponent kitchen:box .

