@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix soma:   <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul:    <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix geo:    <http://www.opengis.net/ont/geosparql#> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr:    <https://w3id.org/psr-task#> .

:instruction1 a psr:Instruction ;
    rdfs:label "Restore the kitchen to an organized state" ;
    psr:hasNaturalLanguage "Restore the kitchen to an organized state by identifying all misplaced items and returning them to their standard storage locations based on their type and function" ;
    psr:hasWorkflow :workflow1 .

:workflow1 a psr:Workflow ;
    psr:hasAction :action1, :action2, :action3, :action4, :action5, :action6, :action7, :action8, :action9, :action10, :action11, :action12 ;
    psr:precedes :action2 :action1 ;
    psr:precedes :action3 :action2 ;
    psr:precedes :action4 :action3 ;
    psr:precedes :action5 :action4 ;
    psr:precedes :action6 :action5 ;
    psr:precedes :action7 :action6 ;
    psr:precedes :action8 :action7 ;
    psr:precedes :action9 :action8 ;
    psr:precedes :action10 :action9 ;
    psr:precedes :action11 :action10 ;
    psr:precedes :action12 :action11 .

:action1 a dul:Action ;
    rdfs:label "Identify misplaced items in the kitchen" ;
    soma:isPerformedBy :agent1 ;
    psr:actsOn :component1 ;
    psr:isAffordedBy :affordance1 .

:action2 a dul:Action ;
    rdfs:label "Pick up orange container" ;
    soma:isPerformedBy :agent1 ;
    psr:actsOn :component2 ;
    psr:isAffordedBy :affordance2 .

:action3 a dul:Action ;
    rdfs:label "Put down orange container in cabinet" ;
    soma:isPerformedBy :agent1 ;
    psr:actsOn :component3 ;
    psr:isAffordedBy :affordance3 .

:action4 a dul:Action ;
    rdfs:label "Pick up bottles and cups" ;
    soma:isPerformedBy :agent1 ;
    psr:actsOn :component4 ;
    psr:isAffordedBy :affordance4 .

:action5 a dul:Action ;
    rdfs:label "Put down bottles and cups in dish drainer" ;
    soma:isPerformedBy :agent1 ;
    psr:actsOn :component5 ;
    psr:isAffordedBy :affordance5 .

:action6 a dul:Action ;
    rdfs:label "Pick up wooden cutting board" ;
    soma:isPerformedBy :agent1 ;
    psr:actsOn :component6 ;
    psr:isAffordedBy :affordance6 .

:action7 a dul:Action ;
    rdfs:label "Put down wooden cutting board in cabinet" ;
    soma:isPerformedBy :agent1 ;
    psr:actsOn :component7 ;
    psr:isAffordedBy :affordance7 .

:action8 a dul:Action ;
    rdfs:label "Pick up boxes of food" ;
    soma:isPerformedBy :agent1 ;
    psr:actsOn :component8 ;
    psr:isAffordedBy :affordance8 .

:action9 a dul:Action ;
    rdfs:label "Put down boxes of food in cabinet" ;
    soma:isPerformedBy :agent1 ;
    psr:actsOn :component9 ;
    psr:isAffordedBy :affordance9 .

:action10 a dul:Action ;
    rdfs:label "Pick up items from shelf on fridge" ;
    soma:isPerformedBy :agent1 ;
    psr:actsOn :component10 ;
    psr:isAffordedBy :affordance10 .

:action11 a dul:Action ;
    rdfs:label "Put down items from shelf on fridge in cabinet" ;
    soma:isPerformedBy :agent1 ;
    psr:actsOn :component11 ;
    psr:isAffordedBy :affordance11 .

:action12 a dul:Action ;
    rdfs:label "Close cabinet" ;
    soma:isPerformedBy :agent1 ;
    psr:actsOn :component12 ;
    psr:isAffordedBy :affordance12 .

:agent1 a dul:Agent ;
    rdfs:label "Robot" .

:component1 a psr:Component ;
    rdfs:label "Kitchen" .

:component2 a psr:Object ;
    rdfs:label "Orange container" .

:component3 a psr:Component ;
    rdfs:label "Cabinet" .

:component4 a psr:Object ;
    rdfs:label "Bottles and cups" .

:component5 a psr:Component ;
    rdfs:label "Dish drainer" .

:component6 a psr:Object ;
    rdfs:label "Wooden cutting board" .

:component7 a psr:Component ;
    rdfs:label "Cabinet" .

:component8 a psr:Object ;
    rdfs:label "Boxes of food" .

:component9 a psr:Component ;
    rdfs:label "Cabinet" .

:component10 a psr:Component ;
    rdfs:label "Shelf on fridge" .

:component11 a psr:Component ;
    rdfs:label "Cabinet" .

:component12 a psr:Component ;
    rdfs:label "Cabinet" .

:affordance1 a psr:Affordance ;
    rdfs:label "Identify" .

:affordance2 a soma:PickingUp ;
    rdfs:label "Pick up" .

:affordance3 a soma:PuttingDown ;
    rdfs:label "Put down" .

:affordance4 a soma:PickingUp ;
    rdfs:label "Pick up" .

:affordance5 a soma:PuttingDown ;
    rdfs:label "Put down" .

