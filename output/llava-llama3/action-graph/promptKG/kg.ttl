@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix soma:   <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul:    <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix geo:    <http://www.opengis.net/ont/geosparql#> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr:    <https://w3id.org/psr-task#> .

:instruction1 a psr:Instruction ;
    rdfs:label "Restore the kitchen to an organized state" ;
    psr:hasNaturalLanguage "Restore the kitchen to an organized state" ;
    psr:hasWorkflow :workflow1 .

:workflow1 a psr:Workflow ;
    psr:hasAction :action1, :action2, :action3, :action4, :action5, :action6, :action7, :action8, :action9, :action10, :action11, :action12, :action13, :action14, :action15 ;
    psr:precedes :action2 :action1 ;
    psr:precedes :action3 :action2 ;
    psr:precedes :action4 :action3 ;
    psr:precedes :action5 :action4 ;
    psr:precedes :action6 :action5 ;
    psr:precedes :action7 :action6 ;
    psr:precedes :action8 :action7 ;
    psr:precedes :action9 :action8 ;
    psr:precedes :action10 :action9 ;
    psr:precedes :action11 :action10 ;
    psr:precedes :action12 :action11 ;
    psr:precedes :action13 :action12 ;
    psr:precedes :action14 :action13 ;
    psr:precedes :action15 :action14 .

:action1 a dul:Action ;
    rdfs:label "Pick up cupcake holder" ;
    soma:isPerformedBy :robot ;
    psr:actsOn :cupcake_holder ;
    psr:isAffordedBy :soma:PickingUp .

:action2 a dul:Action ;
    rdfs:label "Put down cupcake holder in cabinet" ;
    soma:isPerformedBy :robot ;
    psr:actsOn :cupcake_holder ;
    psr:isAffordedBy :soma:PuttingDown .

:action3 a dul:Action ;
    rdfs:label "Pick up small model of house" ;
    soma:isPerformedBy :robot ;
    psr:actsOn :house_model ;
    psr:isAffordedBy :soma:PickingUp .

:action4 a dul:Action ;
    rdfs:label "Put down small model of house in cabinet" ;
    soma:isPerformedBy :robot ;
    psr:actsOn :house_model ;
    psr:isAffordedBy :soma:PuttingDown .

:action5 a dul:Action ;
    rdfs:label "Pick up empty glass jar" ;
    soma:isPerformedBy :robot ;
    psr:actsOn :empty_glass_jar ;
    psr:isAffordedBy :soma:PickingUp .

:action6 a dul:Action ;
    rdfs:label "Put down empty glass jar in cabinet" ;
    soma:isPerformedBy :robot ;
    psr:actsOn :empty_glass_jar ;
    psr:isAffordedBy :soma:PuttingDown .

:action7 a dul:Action ;
    rdfs:label "Pick up white mug" ;
    soma:isPerformedBy :robot ;
    psr:actsOn :white_mug ;
    psr:isAffordedBy :soma:PickingUp .

:action8 a dul:Action ;
    rdfs:label "Put down white mug in cabinet" ;
    soma:isPerformedBy :robot ;
    psr:actsOn :white_mug ;
    psr:isAffordedBy :soma:PuttingDown .

:action9 a dul:Action ;
    rdfs:label "Pick up box of cereal" ;
    soma:isPerformedBy :robot ;
    psr:actsOn :box_of_cereal ;
    psr:isAffordedBy :soma:PickingUp .

:action10 a dul:Action ;
    rdfs:label "Put down box of cereal in cabinet" ;
    soma:isPerformedBy :robot ;
    psr:actsOn :box_of_cereal ;
    psr:isAffordedBy :soma:PuttingDown .

:action11 a dul:Action ;
    rdfs:label "Pick up two cups" ;
    soma:isPerformedBy :robot ;
    psr:actsOn :two_cups ;
    psr:isAffordedBy :soma:PickingUp .

:action12 a dul:Action ;
    rdfs:label "Put down two cups in cabinet" ;
    soma:isPerformedBy :robot ;
    psr:actsOn :two_cups ;
    psr:isAffordedBy :soma:PuttingDown .

:action13 a dul:Action ;
    rdfs:label "Pick up two bottles" ;
    soma:isPerformedBy :robot ;
    psr:actsOn :two_bottles ;
    psr:isAffordedBy :soma:PickingUp .

:action14 a dul:Action ;
    rdfs:label "Put down two bottles in cabinet" ;
    soma:isPerformedBy :robot ;
    psr:actsOn :two_bottles ;
    psr:isAffordedBy :soma:PuttingDown .

:action15 a dul:Action ;
    rdfs:label "Close cabinet" ;
    soma:isPerformedBy :robot ;
    psr:actsOn :cabinet ;
    psr:isAffordedBy :soma:Closing .

:robot a dul:Agent ;
    rdfs:label "Robot" .

:cupcake_holder a psr:Object ;
    rdfs:label "Cupcake holder" .

:house_model a psr:Object ;
    rdfs:label "Small model of house" .

:empty_glass_jar a psr:Object ;
    rdfs:label "Empty glass jar" .

:white_mug a psr:Object ;
    rdfs:label "White mug" .

:box_of_cereal a psr:Object ;
    rdfs:label "Box of cereal" .

:two_cups a psr:Object ;
    rdfs:label "Two cups" .

:two_bottles a psr:Object ;
    rdfs:label "Two bottles" .

:cabinet a psr:Appliance ;
    rdfs:label "Kitchen cabinet" .