    rdfs:label "Pick up" .

:affordance9 a soma:PuttingDown ;
    rdfs:label "Put down" .

:affordance10 a soma:PickingUp ;
    rdfs:label "Pick up" .

:affordance11 a soma:PuttingDown ;
    rdfs:label "Put down" .

:affordance12 a soma:Closing ;
    rdfs:label "Close" .