@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix psr: <https://w3id.org/psr-task#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:task1 a dul:Action ;
    rdfs:label "Identify misplaced items"@en ;
    psr:isAffordedBy :affordance1 .

:affordance1 a psr:Affordance ;
    rdfs:label "Visual inspection"@en .

:task2 a dul:Action ;
    rdfs:label "Pick up misplaced items"@en ;
    psr:isAffordedBy :affordance2 ;
    psr:follows :task1 .

:affordance2 a psr:Affordance ;
    rdfs:label "Grasping"@en .

:task3 a dul:Action ;
    rdfs:label "Put items in standard storage locations"@en ;
    psr:isAffordedBy :affordance3 ;
    psr:follows :task2 .

:affordance3 a psr:Affordance ;
    rdfs:label "Placing"@en .

:component1 a psr:Component ;
    rdfs:label "Kitchen cabinets"@en .

:component2 a psr:Component ;
    rdfs:label "Countertops"@en .

:component3 a psr:Component ;
    rdfs:label "Table"@en .

:task1 psr:actsOn :component1, :component2, :component3 .

:task2 psr:actsOn :component1, :component2, :component3 .

:task3 psr:actsOn :component1, :component2, :component3 .

:instruction1 a psr:Instruction ;
    rdfs:label "Restore kitchen to organized state"@en ;
    psr:hasWorkflow :workflow1 .

:workflow1 a psr:Workflow ;
    rdfs:label "Kitchen organization workflow"@en ;
    psr:hasAction :task1, :task2, :task3 .