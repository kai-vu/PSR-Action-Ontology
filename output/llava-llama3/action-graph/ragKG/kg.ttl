@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix psr: <https://w3id.org/psr-task#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:task1 a dul:Action ;
    rdfs:label "Identify misplaced items"@en ;
    psr:actsOn :kitchen ;
    psr:isAffordedBy soma:Grasping .

:kitchen a psr:Component ;
    rdfs:label "Kitchen"@en .

:task2 a dul:Action ;
    rdfs:label "Pick up misplaced items"@en ;
    psr:actsOn :misplaced_items ;
    psr:isAffordedBy soma:PickingUp ;
    psr:follows :task1 .

:misplaced_items a psr:Component ;
    rdfs:label "Misplaced items"@en .

:task3 a dul:Action ;
    rdfs:label "Put down items in standard storage locations"@en ;
    psr:actsOn :storage_locations ;
    psr:isAffordedBy soma:PuttingDown ;
    psr:follows :task2 .

:storage_locations a psr:Component ;
    rdfs:label "Storage locations"@en .

:instruction1 a psr:Instruction ;
    rdfs:label "Restore kitchen to organized state"@en ;
    psr:hasWorkflow :workflow1 ;
    psr:hasNaturalLanguage "Restore the kitchen to an organized state by identifying all misplaced items and returning them to their standard storage locations based on their type and function."@en .

:workflow1 a psr:Workflow ;
    rdfs:label "Kitchen organization workflow"@en ;
    psr:hasAction :task1 ;
    psr:hasAction :task2 ;
    psr:hasAction :task3 .

:agent1 a dul:Agent ;
    rdfs:label "Robot"@en ;
    soma:isPerformedBy :task1 ;
    soma:isPerformedBy :task2 ;
    soma:isPerformedBy :task3 .