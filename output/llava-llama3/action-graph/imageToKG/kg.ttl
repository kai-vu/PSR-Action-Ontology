@prefix psr: <http://example.org/psr#>
@prefix dul: <http://example.org/dul#>
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>

# Instruction class for task
@namespace psr .

@namespace dul .

# Object class for kitchen items
@namespace dul .

# Action classes with object properties
@namespace psr .

# Affordance class for components
@namespace dul .

# Define affordances
@prefix dul: <http://example.org/dul#>
<http://example.org/dul/Closing> a dul:Affordance ;
   dul:affordanceType "Closing" ;
   dul:component psr:KitchenComponent .

@prefix psr: <http://example.org/psr#>
<http://example.org/psr/RestoringKitchen> a psr:Instruction ;
   psr:naturalLanguage "Restore the kitchen to an organized state by identifying all misplaced items and returning them to their standard storage locations based on their type and function."@en .

# Generate sequence of actions for restoration task
<http://example.org/psr/RestoringKitchen> a psr:Instruction ;
   psr:workflow dul:Workflow .

dul:Workflow a dul:Workflow ;
   dul:hasAction (dul:Appliance a dul:KitchenAppliance) (dul:Furniture a dul:KitchenFurniture) (dul:Object a dul:KitchenItem) .

(dul:Appliance a dul:KitchenAppliance) a dul:Action ;
   psr:precedes (dul:Furniture a dul:KitchenFurniture) .
   psr:isPerformedBy (dul:Agent a dul:Robot) .
   psr:actsOn (dul:Component a dul:Closing) .

(dul:Furniture a dul:KitchenFurniture) a dul:Action ;
   psr:precedes (dul:Object a dul:KitchenItem) .
   psr:isPerformedBy (dul:Agent a dul:Robot) .
   psr:actsOn (dul:Component a dul:Opening) .

(dul:Object a dul:KitchenItem) a dul:Action ;
   psr:precedes (dul:Furniture a dul:KitchenFurniture) .
   psr:isPerformedBy (dul:Agent a dul:Robot) .
   psr:actsOn (dul:Component a dul:Delivering) .