@prefix : <http://example.com/> .

# Class definitions
:Instruction a owl:Class ;
   rdfs:label "Instruction"@en ;
   rdfs:comment "An instruction is a command or directive given to a robot."@en .

:Workflow a owl:Class ;
   rdfs:subClassOf dul:Workflow ;
   rdfs:label "Workflow"@en ;
   rdfs:comment "A workflow is a sequence of tasks or processes that are carried out to fulfil the instruction."@en .

:Action a owl:Class ;
   rdfs:subClassOf dul:Action ;
   rdfs:label "Action"@en ;
   rdfs:comment "An action is an atomic, single, clear action that is carried out to fulfil the instruction."@en .

:Agent a owl:Class ;
   rdfs:subClassOf dul:Agent ;
   rdfs:label "Agent"@en ;
   rdfs:comment "An agent performing an action."@en .

:Component a owl:Class ;
   rdfs:subClassOf dul:Component ;
   rdfs:label "Component"@en ;
   rdfs:comment "A component is a part of an environment that can be used to perform tasks or actions."@en .

:Affordance a owl:Class ;
   rdfs:subClassOf dul:Affordance ;
   rdfs:label "Affordance"@en ;
   rdfs:comment "An affordance is a property of an object that indicates how it can be used."@en .

# Object properties
:hasWorkflow a owl:ObjectProperty ;
   rdfs:domain :Instruction ;
   rdfs:range :Workflow ;
   rdfs:label "has workflow"@en ;
   rdfs:comment "An instruction has a workflow that needs to be executed."@en .

:hasAction a owl:ObjectProperty ;
   rdfs:domain :Workflow ;
   rdfs:range dul:Action ;
   rdfs:label "has action"@en ;
   rdfs:comment "A workflow has one or multiple clear atomic actions."@en .

:precedes a owl:ObjectProperty ;
   rdfs:subPropertyOf dul:precedes ;
   rdfs:domain dul:Action ;
   rdfs:range dul:Action ;
   rdfs:label "precedes" ;
   rdfs:comment "An action can precede another action."@en .

:follows a owl:ObjectProperty ;
   rdfs:subPropertyOf dul:follows ;
   rdfs:domain dul:Action ;
   rdfs:range dul:Action ;
   rdfs:label "follow" ;
   rdfs:comment "An action can follow another action."@en .

# Instances
:Instruction_1 a owl:NamedIndividual ;
   :hasNaturalLanguage "Reorganise the kitchen" ;
   :hasWorkflow :Workflow_1 ;
   rdfs:label "Reorganise the kitchen instruction"@en .

:Workflow_1 a dul:Workflow ;
   rdfs:label "Kitchen reorganisation workflow"@en .

:Action_1 a dul:Action ;
   rdfs:label "Open drawer"@en ;
   :isPerformedBy :Agent_1 ;
   :actsOn :Component_1 ;
   :follows :Action_2 ;
   rdfs:comment "The robot opens the drawer."@en .

:Action_2 a dul:Action ;
   rdfs:label "Close drawer"@en ;
   :isPerformedBy :Agent_1 ;
   :actsOn :Component_2 ;
   rdfs:comment "The robot closes the drawer."@en .

:Agent_1 a dul:Agent ;
   rdfs:label "Robot"@en .

:Component_1 a dul:Component ;
   rdfs:label "Drawer"@en ;
   :isAffordedBy :Affordance_1 ;
   rdfs:comment "The drawer is a component that the robot can interact with."@en .

:Component_2 a dul:Component ;
   rdfs:label "Drawer"@en ;
   :isAffordedBy :Affordance_2 ;
   rdfs:comment "The drawer is a component that the robot can interact with."@en .

:Affordance_1 a dul:Affordance ;
   rdfs:label "Opening"@en ;
   rdfs:comment "The affordance to open the drawer."@en .

:Affordance_2 a dul:Affordance ;
   rdfs:label "Closing"@en ;
   rdfs:comment "The affordance to close the drawer."@en .