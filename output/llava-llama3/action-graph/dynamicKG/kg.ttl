@prefix kitchen: <http://example.org/kitchen#> .

kitchen:assistant kitchen:CLOSES kitchen:cabinet_door,
        kitchen:cabinet_or_pantry_door ;
    kitchen:MOVES_TO kitchen:cabinets_on_the_far_left_wall_of_the_kitchen,
        kitchen:countertops_near_the_sink,
        kitchen:pantry_or_storage_area,
        kitchen:table ;
    kitchen:OPENS kitchen:cabinet_door,
        kitchen:cabinet_or_pantry_door ;
    kitchen:PICKS_UP kitchen:box_of_cereal,
        kitchen:cupcake_holder,
        kitchen:empty_glass_jar,
        kitchen:small_model_of_a_house,
        kitchen:two_bottles,
        kitchen:two_cups,
        kitchen:white_mug ;
    kitchen:PLACES kitchen:box_of_cereal,
        kitchen:cupcake_holder,
        kitchen:empty_glass_jar,
        kitchen:small_model_of_a_house,
        kitchen:two_bottles,
        kitchen:two_cups,
        kitchen:white_mug .

kitchen:box_of_cereal kitchen:IS_IN kitchen:cabinet_or_pantry ;
    kitchen:IS_ON kitchen:countertop .

kitchen:cupcake_holder kitchen:IS_ON kitchen:countertop,
        kitchen:table .

kitchen:empty_glass_jar kitchen:IS_IN kitchen:cabinet ;
    kitchen:IS_ON kitchen:table .

kitchen:small_model_of_a_house kitchen:IS_ON kitchen:open_shelf_unit,
        kitchen:table .

kitchen:two_bottles kitchen:ARE_IN kitchen:cabinet ;
    kitchen:ARE_ON kitchen:countertops_near_the_stove_and_the_fridge .

kitchen:two_cups kitchen:ARE_IN kitchen:cabinet ;
    kitchen:ARE_ON kitchen:countertops_on_each_side_of_the_sink .

kitchen:white_mug kitchen:IS_IN kitchen:cabinet ;
    kitchen:IS_ON kitchen:table .

