@prefix kitchen: <http://example.org/kitchen#> .

kitchen:assistant kitchen:APPROACHES kitchen:cabinet_with_glass_doors,
        kitchen:dining_table,
        kitchen:sink,
        kitchen:small_shelf_on_top_of_fridge,
        kitchen:small_shelf_to_left_of_fridge,
        kitchen:white_surface_on_top_of_fridge_to_its_right ;
    kitchen:CONFIRMS kitchen:kitchen_is_restored_to_organized_state ;
    kitchen:EXAMINES kitchen:dish_drainer,
        kitchen:items_in_cabinet,
        kitchen:items_on_dining_table,
        kitchen:items_on_sink_counter,
        kitchen:items_on_small_shelf,
        kitchen:items_on_white_surface ;
    kitchen:MOVES_TO kitchen:kitchen_area ;
    kitchen:PICKS_UP kitchen:bottles,
        kitchen:boxes_of_food,
        kitchen:cups,
        kitchen:dishes_or_utensils,
        kitchen:kitchen-related_items,
        kitchen:misplaced_items,
        kitchen:misplaced_kitchen_items,
        kitchen:orange_container ;
    kitchen:REMOVES kitchen:misplaced_kitchen_items,
        kitchen:wooden_cutting_board ;
    kitchen:VERIFIES kitchen:all_items_in_kitchen_and_dining_area .

kitchen:bottles kitchen:PLACED_IN kitchen:cabinet_below_white_countertop .

kitchen:boxes_of_food kitchen:PLACED_IN kitchen:cabinet_below_white_countertop_or_designated_food_storage_area .

kitchen:cups kitchen:PLACED_IN kitchen:cabinet_below_white_countertop .

kitchen:dishes_or_utensils kitchen:PLACED_IN kitchen:cabinet_below_white_countertop_or_designated_dish_storage_area .

kitchen:kitchen-related_items kitchen:PLACED_IN kitchen:standard_storage_locations .

kitchen:misplaced_items kitchen:PLACED_IN kitchen:standard_storage_locations .

kitchen:orange_container kitchen:PLACED_IN kitchen:cabinet_below_white_countertop_or_designated_container_storage_area .

kitchen:wooden_cutting_board kitchen:PLACED_IN kitchen:cabinet_below_white_countertop_or_designated_cutting_board_storage_area .

kitchen:misplaced_kitchen_items kitchen:PLACED_IN kitchen:standard_storage_locations .

