psr:ActionRetrieveDishOfBread a dul:Action ;
    rdfs:label "Retrieve Dish of Sliced Bread"@en ;
    psr:isPerformedBy psr:RobotAgent ;
    psr:actsOn psr:SlicedBreadDish .

psr:ActionRetrieveWaterBottle a dul:Action ;
    rdfs:label "Retrieve Water Bottle"@en ;
    psr:isPerformedBy psr:RobotAgent ;
    psr:actsOn psr:WaterBottle .

psr:ActionRetrieveYellowCup a dul:Action ;
