psr:ActionRetrieveJugCreamer a dul:Action ;
    rdfs:label "Retrieve Jug/Creamer"@en ;
    psr:isPerformedBy psr:RobotAgent ;
    psr:actsOn psr:JugCreamer .

# Returning actions (assuming return to original storage locations)
psr:ActionReturnJELLO a dul:Action ;
    rdfs:label "Return JELLO box to storage"@en ;
    psr:isPerformedBy psr:RobotAgent ;
    psr:actsOn psr:JELLO .

