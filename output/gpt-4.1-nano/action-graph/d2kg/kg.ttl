@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix soma:   <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul:    <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix geo:    <http://www.opengis.net/ont/geosparql#> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr:    <https://w3id.org/psr-task#> .
@prefix :       <https://example.org/data/> .

# Define the workflow for restoring the kitchen
psr:kitchenRestorationWorkflow a owl:Class, dul:Workflow ;
    rdfs:label "Kitchen Restoration Workflow"@en .

# Actions involved in the process
psr:RetrieveMisplacedItems a owl:Class, dul:Action ;
    rdfs:label "Retrieve Misplaced Items"@en .

psr:ReturnItemsToStorage a owl:Class, dul:Action ;
    rdfs:label "Return Items to Storage"@en .

# Agents
psr:RobotAgent a owl:Class, dul:Agent ;
    rdfs:label "Robot Agent"@en .

# Define the instruction
psr:RestoreKitchenInstruction a owl:Class, psr:Instruction ;
    rdfs:label "Restore the kitchen to an organized state by retrieving misplaced items and returning them to storage"@en ;
    psr:hasNaturalLanguage "Restore the kitchen to an organized state by identifying all misplaced items and returning them to their standard storage locations based on their type and function."@en ;
    psr:hasWorkflow psr:kitchenRestorationWorkflow .

# Define the workflow sequence
psr:kitchenRestorationWorkflow
    rdfs:subClassOf dul:Workflow ;
    rdfs:comment "Sequential workflow to restore kitchen organization."@en .

# Define sequence of actions
psr:RetrieveAndReturnSequence a owl:NamedIndividual ;
    a owl:Sequence ;
    rdfs:comment "Sequence of actions to retrieve and return misplaced items."@en .

# Link actions to workflow
psr:kitchenRestorationWorkflow
    psr:hasAction psr:RetrieveMisplacedItems, psr:ReturnItemsToStorage .

# Actions details
psr:RetrieveMisplacedItems
    psr:actsOn psr:BoxOfJELLO, psr:BoxOfRiceBubbles, psr:SmallRedContainerStrawberryJam, psr:RedWhiteContainerBloodOrangePreserves, psr:SmallDishOfBread, psr:WaterBottle, psr:YellowCup, psr:GlassOfWine, psr:JugCremater ;
    rdfs:comment "Identify and retrieve all misplaced objects on the countertop and nearby."@en .

psr:ReturnItemsToStorage
    psr:actsOn psr:Object ;
    rdfs:comment "Return all retrieved items to their respective standard storage locations."@en .

# Example object instances (assuming their types based on description)
psr:JELLO a psr:Object, dul:Entity ;
    rdfs:label "JELLO box"@en .

psr:RiceBubbles a psr:Object, dul:Entity ;
    rdfs:label "Rice bubbles box"@en .

psr:StrawberryJam a psr:Object, dul:Entity ;
    rdfs:label "Strawberry jam container"@en .

psr:BloodOrangePreserves a psr:Object, dul:Entity ;
    rdfs:label "Blood orange preserves container"@en .

psr:SlicedBreadDish a psr:Object, dul:Entity ;
    rdfs:label "Dish of sliced bread"@en .

psr:WaterBottle a psr:Object, dul:Entity ;
    rdfs:label "Water bottle"@en .

psr:YellowCup a psr:Object, dul:Entity ;
    rdfs:label "Yellow cup"@en .

psr:WineGlass a psr:Object, dul:Entity ;
    rdfs:label "Glass of wine"@en .

psr:JugCreamer a psr:Object, dul:Entity ;
    rdfs:label "Jug/Creamer"@en .

# Define actions as instances
psr:ActionRetrieveJELLO a dul:Action ;
    rdfs:label "Retrieve JELLO box"@en ;
    psr:isPerformedBy psr:RobotAgent ;
    psr:actsOn psr:JELLO .

psr:ActionRetrieveRiceBubbles a dul:Action ;
    rdfs:label "Retrieve Rice Bubbles"@en ;
    psr:isPerformedBy psr:RobotAgent ;
    psr:actsOn psr:RiceBubbles .

psr:ActionRetrieveStrawberryJam a dul:Action ;
    rdfs:label "Retrieve Strawberry Jam"@en ;
