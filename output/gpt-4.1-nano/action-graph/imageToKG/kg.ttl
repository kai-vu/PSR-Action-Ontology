@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix soma:   <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul:    <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix geo:    <http://www.opengis.net/ont/geosparql#> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr:    <https://w3id.org/psr-task#> .

#################### Actions Sequence ####################

[] a psr:Instruction ;
    psr:hasNaturalLanguage "Reorganize the kitchen"@en ;
    psr:hasWorkflow [
        a psr:Workflow ;
        psr:hasAction [
            a dul:Action ;
            soma:isPerformedBy :Robot ;
            psr:actsOn :Kitchen ;
            psr:isAffordedBy :OpeningDoorCabinet ;
        ] ;
        psr:hasAction [
            a dul:Action ;
            soma:isPerformedBy :Robot ;
            psr:actsOn :ObjectToRearrange ;
            psr:isAffordedBy :Grasping ;
        ] ;
        psr:hasAction [
            a dul:Action ;
            soma:isPerformedBy :Robot ;
            psr:actsOn :ObjectToRearrange ;
            psr:isAffordedBy :PuttingDown ;
        ] ;
        psr:hasAction [
            a dul:Action ;
            soma:isPerformedBy :Robot ;
            psr:actsOn :ObjectToRearrange ;
            psr:isAffordedBy :Closing ;
        ] ;
        psr:hasAction [
            a dul:Action ;
            soma:isPerformedBy :Robot ;
            psr:actsOn :AnotherObject ;
            psr:isAffordedBy :Grasping ;
        ] ;
        psr:hasAction [
            a dul:Action ;
            soma:isPerformedBy :Robot ;
            psr:actsOn :AnotherObject ;
            psr:isAffordedBy :PuttingDown ;
        ] ;
        psr:hasAction [
            a dul:Action ;
            soma:isPerformedBy :Robot ;
            psr:actsOn :ObjectToRearrange ;
            psr:isAffordedBy :Closing ;
        ] ;
    ] .