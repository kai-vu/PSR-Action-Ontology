@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix geo:    <http://www.opengis.net/ont/geosparql#> .
@prefix soma:   <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul:    <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr:    <https://w3id.org/psr-task#> .

# Define environment
psr:KitchenEnvironment a psr:Environment ;
    rdfs:label "Kitchen Environment"@en .
psr:DiningAreaEnvironment a psr:Environment ;
    rdfs:label "Dining Area Environment"@en .

# Components in the environment
psr:KitchenCabinet a psr:Appliance ;
    rdfs:label "Kitchen Cabinet"@en .

psr:MarbleCountertop a psr:Furniture ;
    rdfs:label "Marble Countertop"@en .

psr:FoodItems a psr:Object ;
    rdfs:label "Food Items"@en .

psr:PlateStack a psr:Object ;
    rdfs:label "Stack of Plates"@en .

psr:JELLOBox a psr:Object ;
    rdfs:label "Box of JELLO"@en .

psr:RiceBubblesCarton a psr:Object ;
    rdfs:label "Carton of Rice Bubbles"@en .

psr:StrawberryJamContainer a psr:Object ;
    rdfs:label "Container of Strawberry Jam"@en .

psr:StrawberryJamRedWhite a psr:Object ;
    rdfs:label "Red and White Strawberry Jam Container"@en .

psr:AlpenSaltBox a psr:Object ;
    rdfs:label "Box of Alpen Salz"@en .

psr:BonneMamanJar a psr:Object ;
    rdfs:label "Jar of Bonne Maman Strawberry Preserves"@en .

psr:Refrigerator a psr:Appliance ;
    rdfs:label "White Refrigerator"@en .

psr:WallMountedVent a psr:Object ;
    rdfs:label "Overhead Wooden Vent"@en .

psr:DiningTable a psr:Furniture ;
    rdfs:label "Wooden Dining Table"@en .

psr:EvianBottle a psr:Object ;
    rdfs:label "Bottle of Evian Water"@en .

psr:DiningBoxRiceBubbles a psr:Object ;
    rdfs:label "Box of Rice Bubbles"@en .

psr:JamJar a psr:Object ;
    rdfs:label "Jar of Jam"@en .

psr:OrangeMug a psr:Object ;
    rdfs:label "Orange Ceramic Mug"@en .

psr:WineGlass a psr:Object ;
    rdfs:label "Wine Glass"@en .

psr:WhitePitcher a psr:Object ;
    rdfs:label "Large White Pitcher"@en .

# Locations
psr:LeftSide of the environment a psr:Location ;
    rdfs:label "Left Side"@en .

psr:CornerPosition a psr:Location ;
    rdfs:label "Corner Position"@en .

psr:AgainstWhiteWall a psr:Location ;
    rdfs:label "Against White Wall"@en .

psr:WallMeetingCorner a psr:Location ;
    rdfs:label "Wall Meeting Corner"@en .

psr:PositionNearWall a psr:Location ;
    rdfs:label "Position Near Wall"@en .

psr:PositionAgainstWall a psr:Location ;
    rdfs:label "Position Against Wall"@en .

# Relations establishing environment components
psr:hasComponent psr:KitchenEnvironment psr:KitchenCabinet, psr:MarbleCountertop, psr:Refrigerator, psr:WallMountedVent, psr:DiningTable, psr:WhitePitcher, psr:EvianBottle, psr:DiningBoxRiceBubbles, psr:JamJar, psr:OrangeMug, psr:WineGlass .

# Food items on the marble countertop
geo:sfContains psr:MarbleCountertop psr:PlateStack, psr:JELLOBox, psr:RiceBubblesCarton, psr:StrawberryJamContainer .

# Specific food containers
geo:sfContains psr:StrawberryJamContainer psr:StrawberryJamRedWhite .
geo:sfContains psr:StrawberryJamContainer psr:AlpenSaltBox .
geo:sfContains psr:StrawberryJamContainer psr:BonneMamanJar .

# Refrigerator location
dul:hasLocation psr:Refrigerator psr:CornerPosition .

# Vent mounted above the countertop
dul:hasLocation psr:WallMountedVent psr:PositionNearWall .

# Dining area Components
psr:DiningTable a psr:Furniture ;
    rdfs:label "Wooden Dining Table"@en .

psr:BottleEvian a psr:Object ;
    rdfs:label "Bottle of Evian Water"@en .

psr:BoxRiceBubbles a psr:Object ;
    rdfs:label "Box of Rice Bubbles"@en .

psr:JamJarObject a psr:Object ;
    rdfs:label "Jar of Jam"@en .

psr:OrangeCeramicMug a psr:Object ;
    rdfs:label "Orange Ceramic Mug"@en .

psr:WineGlassObject a psr:Object ;
    rdfs:label "Wine Glass"@en .

psr:LargeWhitePitcher a psr:Object ;
    rdfs:label "Large White Pitcher"@en .

# Positions of objects relative to the environment
geo:sfWithin psr:EvianBottle psr:PositionNearWall .
geo:sfWithin psr:BoxRiceBubbles psr:PositionNearWall .
geo:sfWithin psr:JamJar psr:PositionNearWall .
geo:sfWithin psr:OrangeMug psr:PositionNearWall .
geo:sfWithin psr:WineGlass psr:PositionNearWall .
geo:sfWithin psr:WhitePitcher psr:PositionNearWall .

# Objects on the dining table
geo:sfContains psr:DiningTable psr:EvianBottle, psr:BoxRiceBubbles, psr:JamJarObject, psr:OrangeCeramicMug, psr:WineGlassObject, psr:LargeWhitePitcher .

# Additional possible spatial relations (example)
# Assuming positions, these can be further detailed if needed, e.g.,
# geo:sfOverlaps or geo:sfContains between objects and relative positions.

# Note: The specific spatial relations between all objects are context-dependent and can be elaborated further if more detailed positional data is provided.