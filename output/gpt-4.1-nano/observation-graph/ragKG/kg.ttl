@prefix env: <http://www.semanticdesktop.org/ontologies/environment#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

env:Environment a env:PhysicalEnvironment ;
    env:hasPart env:Kitchen, env:DiningArea, env:Floor, env:Walls, env:Ceiling .

env:Kitchen a env:Room ;
    env:hasFeature env:AdjacentToDiningArea ;
    env:contains env:KitchenCabinet, env:Countertop, env:Refrigerator, env:OverheadVent .

env:DiningArea a env:Room ;
    env:hasFeature env:AdjacentToKitchen ;
    env:contains env:Table .

env:Floor a env:FloorMaterial ;
    env:hasColor "dark brown" .

env:Walls a env:Wall ;
    env:hasColor "white" .

env:Ceiling a env:Ceiling ;
    env:hasLighting env:CeilingLights .

env:KitchenCabinet a env:Furnishing ;
    env:hasColor "black" .

env:Countertop a env:Counter ;
    env:hasMaterial env:Marble ;
    env:hasColor "beige" ;
    env:contains env:FoodItems .

env:FoodItems a env:Food ;
    env:includes env:WhitePlates, env:JELLO, env:RiceBubbles, env:StrawberryJam .

env:WhitePlates a env:FoodItem ;
    env:hasColor "white" .

env:JELLO a env:FoodItem .

env:RiceBubbles a env:FoodItem .

env:StrawberryJam a env:FoodItem ;
    env:hasBrand env:BonneMaman .

env:BonneMaman a env:Brand .

env:Refrigerator a env:Appliance ;
    env:locatedAt env:Corner .

env:OverheadVent a env:Vent ;
    env:mountedOn env:Wall .

env:Table a env:Furnishing ;
    env:hasShape env:Rectangle ;
    env:hasMaterial env:Wood ;
    env:hasColor "light wood" ;
    env:contains env:TableContents .

env:TableContents a env:Contents ;
    env:includes env:BottleEvian, env:BoxRiceBubbles, env:JarJam, env:OrangeMug, env:WineGlass, env:WhitePitcher .

env:BottleEvian a env:Container ;
    env:hasBrand "Evian" .

env:BoxRiceBubbles a env:Container ;
    env:hasBrand "Rice Bubbles" .

env:JarJam a env:Container ;
    env:hasBrand env:BonneMaman .

env:OrangeMug a env:Container ;
    env:hasColor "orange" .

env:WineGlass a env:Container ;
    env:hasShape env:Cylinder .

env:WhitePitcher a env:Container ;
    env:hasShape env:Cylinder .

@prefix env: <http://www.semanticdesktop.org/ontologies/environment#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# The above triples describe the environment, including the kitchen, dining area, objects, and their attributes within the environment as per the given description.