@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix geo:    <http://www.opengis.net/ont/geosparql#> .
@prefix soma:   <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul:    <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr:    <https://w3id.org/psr-task#> .

# Environment
<http://example.org/environment> a psr:Environment ;
    rdfs:label "Kitchen Environment"@en .

# Kitchen Area
<http://example.org/kitchenArea> a psr:Environment ;
    rdfs:label "Kitchen Area"@en .

# Cabinets
<http://example.org/cabinets> a psr:Component ;
    rdfs:label "Kitchen Cabinets"@en ;
    dul:hasLocation <http://example.org/kitchenArea> .

# Countertop
<http://example.org/countertop> a psr:Component ;
    rdfs:label "Countertop"@en ;
    dul:hasLocation <http://example.org/kitchenArea> .

# Food Items and Containers on Countertop
<http://example.org/boxJello> a psr:Object ;
    rdfs:label "Box of Jello"@en ;
    dul:hasLocation <http://example.org/countertop> .

<http://example.org/boxRiceBubbles> a psr:Object ;
    rdfs:label "Box of Rice Bubbles"@en ;
    dul:hasLocation <http://example.org/countertop> .

<http://example.org/redLiddedContainer> a psr:Object ;
    rdfs:label "Red-lidded Container"@en ;
    dul:hasLocation <http://example.org/countertop> .

<http://example.org/orangeContainer> a psr:Object ;
    rdfs:label "Orange Container"@en ;
    dul:hasLocation <http://example.org/countertop> .

<http://example.org/waterBottle> a psr:Object ;
    rdfs:label "Water Bottle"@en ;
    dul:hasLocation <http://example.org/countertop> .

<http://example.org/smallRedBox> a psr:Object ;
    rdfs:label "Small Red Box"@en ;
    dul:hasLocation <http://example.org/countertop> .

<http://example.org/wineGlass> a psr:Object ;
    rdfs:label "Wine Glass"@en ;
    dul:hasLocation <http://example.org/countertop> .

<http://example.org/whitePitcher> a psr:Object ;
    rdfs:label "White Pitcher"@en ;
    dul:hasLocation <http://example.org/countertop> .

# Fridge
<http://example.org/fridge> a psr:Object ;
    rdfs:label "White Fridge"@en ;
    dul:hasLocation <http://example.org/kitchenArea> .

# Fridge Handle
<http://example.org/fridgeHandle> a psr:Object ;
    rdfs:label "Fridge Handle"@en ;
    dul:hasLocation <http://example.org/fridge> .

# Door leading to another room
<http://example.org/doorway> a psr:Object ;
    rdfs:label "Doorway"@en ;
    dul:hasLocation <http://example.org/kitchenArea> .

# Wooden Door
<http://example.org/woodenDoor> a psr:Object ;
    rdfs:label "Wooden Door"@en ;
    dul:hasLocation <http://example.org/doorway> .

# Door Handle
<http://example.org/doorHandle> a psr:Object ;
    rdfs:label "Door Handle"@en ;
    dul:hasLocation <http://example.org/woodenDoor> .

# Walls
<http://example.org/walls> a psr:Object ;
    rdfs:label "White Textured Walls"@en ;
    dul:hasLocation <http://example.org/kitchenArea> .

# Flooring
<http://example.org/floor> a psr:Object ;
    rdfs:label "Wooden Parquet Floor"@en ;
    dul:hasLocation <http://example.org/kitchenArea> .

# Ceiling Light
<http://example.org/ceilingLight> a psr:Object ;
    rdfs:label "Ceiling Light Fixture"@en ;
    dul:hasLocation <http://example.org/kitchenArea> .

# Wall-mounted Shelf
<http://example.org/wallShelf> a psr:Object ;
    rdfs:label "Wall-mounted Wooden Shelf"@en ;
    dul:hasLocation <http://example.org/kitchenArea> .

# Speaker or Vent underneath Shelf
<http://example.org/speakerVent> a psr:Object ;
    rdfs:label "Speaker/Vent Under Shelf"@en ;
    dul:hasLocation <http://example.org/wallShelf> .

# Room setup for the same environment - multiple angles
# (Note: position relations are optional, but can be added if detailed)
# For example, relate kitchen area to environment
<http://example.org/environment> dul:hasComponent <http://example.org/kitchenArea> .

# Positions (optional, showing relative setup)
<http://example.org/countertopPosition> geo:sfContains <http://example.org/countertop> .
<http://example.org/fridgePosition> geo:sfContains <http://example.org/fridge> .
<http://example.org/doorwayPosition> geo:sfContains <http://example.org/doorway> .
<http://example.org/wallsPosition> geo:sfContains <http://example.org/walls> .
<http://example.org/floorPosition> geo:sfContains <http://example.org/floor> .
<http://example.org/ceilingLightPosition> geo:sfContains <http://example.org/ceilingLight> .
<http://example.org/wallShelfPosition> geo:sfContains <http://example.org/wallShelf> .
<http://example.org/speakerVentPosition> geo:sfContains <http://example.org/speakerVent> .

# Affordances
<http://example.org/door> a psr:Object, soma:Opening ;
    rdfs:label "Door"@en ;
    psr:hasAffordance soma:Opening ;
    dul:hasLocation <http://example.org/woodenDoor> .

<http://example.org/fridge> a psr:Object, soma:Closing ;
    rdfs:label "Refrigerator"@en ;
    psr:hasAffordance soma:Closing ;
    dul:hasLocation <http://example.org/fridge> .

<http://example.org/handleFridge> a psr:Object, soma:Pushing ;
    rdfs:label "Fridge Handle"@en ;
    psr:hasAffordance soma:Pushing ;
    dul:hasLocation <http://example.org/fridgeHandle> .

<http://example.org/doorHandle> a psr:Object, soma:Pushing ;
    rdfs:label "Door Handle"@en ;
    psr:hasAffordance soma:Pushing ;
    dul:hasLocation <http://example.org/doorHandle> .

# Notes: The entities and relations are coded following the ontology schema, including locations, components, and affordances, to form a structured Knowledge Graph.