@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix geo:    <http://www.opengis.net/ont/geosparql#> .
@prefix soma:   <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul:    <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr:    <https://w3id.org/psr-task#> .

###################################
# Environment
###################################
psr:Kitchen a psr:Environment ;
    rdfs:label "Kitchen Environment"@en ;
    dul:hasComponent psr:Cabinet, psr:Refrigerator, psr:Table .

###################################
# Components
###################################
psr:Cabinet a psr:Furniture ;
    rdfs:label "Kitchen Cabinet"@en ;
    dul:hasLocation psr:CabinetLocation .

psr:Refrigerator a psr:Appliance ;
    rdfs:label "Refrigerator"@en ;
    dul:hasLocation psr:RefrigeratorLocation ;
    psr:hasAffordance soma:Opening, soma:Closing .

psr:Table a psr:Furniture ;
    rdfs:label "Dining Table"@en ;
    dul:hasLocation psr:TableLocation .

psr:KitchenCounter a psr:Furniture ;
    rdfs:label "Kitchen Counter"@en ;
    dul:hasLocation psr:CounterLocation .

psr:DishShelf a psr:Furniture ;
    rdfs:label "Dish Shelf"@en ;
    dul:hasLocation psr:DishShelfLocation .

psr:Door a psr:Object ;
    rdfs:label "Refrigerator Door"@en ;
    psr:hasAffordance soma:Opening, soma:Closing .

psr:CounterDrawers a psr:Furniture ;
    rdfs:label "Counter Drawers"@en ;
    dul:hasLocation psr:CounterDrawersLocation .

###################################
# Locations
###################################
psr:CabinetLocation a soma:Location ;
    rdfs:label "Cabinet Location"@en .

psr:RefrigeratorLocation a soma:Location ;
    rdfs:label "Refrigerator Location"@en .

psr:TableLocation a soma:Location ;
    rdfs:label "Table Location"@en .

psr:CounterLocation a soma:Location ;
    rdfs:label "Counter Location"@en .

psr:DishShelfLocation a soma:Location ;
    rdfs:label "Dish Shelf Location"@en .

psr:CounterDrawersLocation a soma:Location ;
    rdfs:label "Counter Drawers Location"@en .

###################################
# Objects
###################################
psr:Bottle a psr:Object ;
    rdfs:label "Water Bottle"@en ;
    dul:hasLocation psr:BottleLocation ;
    psr:hasAffordance soma:Holding, soma:Pushing .

psr:Box of Jello a psr:Object ;
    rdfs:label "Box of Jello"@en ;
    dul:hasLocation psr:JelloBoxLocation ;
    psr:hasAffordance soma:PickingUp, soma:PuttingDown .

psr:Box of Shortbread a psr:Object ;
    rdfs:label "Box of Shortbread"@en ;
    dul:hasLocation psr:ShortbreadBoxLocation ;
    psr:hasAffordance soma:PickingUp, soma:PuttingDown .

psr:Spoon a psr:Object ;
    rdfs:label "Spoon"@en ;
    dul:hasLocation psr:SpoonLocation ;
    psr:hasAffordance soma:Grasping .

psr:Cup a psr:Object ;
    rdfs:label "Wine Glass"@en ;
    dul:hasLocation psr:CupLocation ;
    psr:hasAffordance soma:Holding, soma:PuttingDown .

psr:Jug a psr:Object ;
    rdfs:label "Jug"@en ;
    dul:hasLocation psr:JugLocation ;
    psr:hasAffordance soma:Holding, soma:PuttingDown .

psr:FoodItem a psr:Object ;
    rdfs:label "Food Item"@en ;
    dul:hasLocation psr:FoodLocation .

###################################
# Locations for objects
###################################
psr:BottleLocation a soma:Location ;
    rdfs:label "Bottle Location"@en .

psr:JelloBoxLocation a soma:Location ;
    rdfs:label "Jello Box Location"@en .

psr:ShortbreadBoxLocation a soma:Location ;
    rdfs:label "Shortbread Box Location"@en .

psr:SpoonLocation a soma:Location ;
    rdfs:label "Spoon Location"@en .

psr:CupLocation a soma:Location ;
    rdfs:label "Cup Location"@en .

psr:JugLocation a soma:Location ;
    rdfs:label "Jug Location"@en .

psr:FoodLocation a soma:Location ;
    rdfs:label "Food Item Location"@en .

###################################
# Affordances for Components
###################################
psr:RefrigeratorHasDoor a owl:NamedIndividual ;
    a soma:Opening ;
    rdfs:label "Refrigerator Door Opening"@en .

psr:RefrigeratorCanClose a owl:NamedIndividual ;
    a soma:Closing ;
    rdfs:label "Refrigerator Can Close"@en .

# End of Knowledge Graph