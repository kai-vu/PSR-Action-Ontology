@prefix : <http://example.org/ontology#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:Environment a :PhysicalEnvironment ;
    :hasArea :KitchenArea ;
    :hasWall :WhiteTexturedWalls ;
    :hasFloor :WoodenParquetFloor ;
    :hasCeiling :CeilingWithLightFixture ;
    :hasRoom :AdjacentRoom .

:KitchenArea a :Room ;
    :contains :Cabinets, :Countertop, :FoodItems, :Containers, :Fridge, :Doorway, :Shelf .

:Cabinets a :Furniture ; 
    :hasColor "dark-colored"^^xsd:string .

:Countertop a :Surface ;
    :hasColor "beige"^^xsd:string .

:FoodItems a :Food ;
    :includes :Jello, :RiceBubbles .

:Containers a :Container ;
    :includes :RedLiddedContainer, :OrangeContainer, :WaterBottle, :SmallRedBox, :WineGlass, :WhitePitcher , :Label .

:RedLiddedContainer a :Container ;
    :hasLabel :Label .

:Label a :Text .

:Fridge a :ElectricalAppliance ;
    :locatedAt :LeftSideOfCounter ;
    :hasColor "white"^^xsd:string ;
    :hasHandle :RightSideHandle .

:LeftSideOfCounter a :Location .

:Doorway a :Opening ;
    :leadsTo :OtherRoom ;
    :hasDoor :WoodenDoor .

:WoodenDoor a :Door ;
    :hasHandle :DoorHandle .

:DoorHandle a :Handle .

:Wall a :Structure ;
    :hasTexture "white"^^xsd:string .

:Shelf a :WallMountedFurniture ;
    :hasUnderneath :SpeakerOrVent .

:SpeakerOrVent a :VentOrSpeaker .

:CeilingWithLightFixture a :Ceiling ;
    :hasLightFixture :SimpleRoundLightFixture .

:SimpleRoundLightFixture a :LightFixture .