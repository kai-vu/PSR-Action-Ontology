@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/psr-task#> .

psr:kitchen a psr:Environment ;
  rdfs:label "kitchen"@en .

psr:refrigerator a psr:Appliance, psr:Component ;
  rdfs:label "refrigerator"@en ;
  psr:hasLocation psr:kitchen .

psr:table a psr:Furniture, psr:Component ;
  rdfs:label "table"@en ;
  psr:hasLocation psr:diningRoom .

psr:diningRoom a psr:Environment ;
  rdfs:label "dining room"@en .

psr:chair1 a psr:Furniture, psr:Component ;
  rdfs:label "chair"@en ;
  psr:hasLocation psr:diningRoom .

psr:chair2 a psr:Furniture, psr:Component ;
  rdfs:label "chair"@en ;
  psr:hasLocation psr:diningRoom .

psr:chair3 a psr:Furniture, psr:Component ;
  rdfs:label "chair"@en ;
  psr:hasLocation psr:diningRoom .

psr:cabinet a psr:Component, psr:Furniture ;
  rdfs:label "cabinet"@en ;
  psr:hasLocation psr:kitchen .

psr:cupboard a psr:Component, psr:Furniture ;
  rdfs:label "cupboard"@en ;
  psr:hasLocation psr:kitchen .

psr:box_of_rice_bubbles a psr:Object ;
  rdfs:label "box of Rice Bubbles"@en ;
  psr:hasLocation psr:kitchen .

psr:box_of_jello a psr:Object ;
  rdfs:label "box of Jello"@en ;
  psr:hasLocation psr:kitchen .

psr:jar_of_jam a psr:Object ;
  rdfs:label "jar of jam"@en ;
  psr:hasLocation psr:kitchen .

psr:evian_bottle a psr:Object ;
  rdfs:label "Evian bottle"@en ;
  psr:hasLocation psr:diningRoom .

psr:book1 a psr:Object ;
  rdfs:label "book1"@en ;
  psr:hasLocation psr:diningRoom .

psr:book2 a psr:Object ;
  rdfs:label "book2"@en ;
  psr:hasLocation psr:kitchen .

psr:plate a psr:Object ;
  rdfs:label "plate"@en ;
  psr:hasLocation psr:kitchen .

psr:cup a psr:Object ;
  rdfs:label "cup"@en ;
  psr:hasLocation psr:kitchen .

psr:jar_of_marmite a psr:Object ;
  rdfs:label "jar of marmite"@en ;
  psr:hasLocation psr:kitchen .

psr:alpen_salt a psr:Object ;
  rdfs:label "Alpen Salt"@en ;
  psr:hasLocation psr:kitchen .

dul:hasComponent(psr:kitchen, psr:refrigerator) .
dul:hasComponent(psr:kitchen, psr:cabinet) .
dul:hasComponent(psr:kitchen, psr:cupboard) .
dul:hasComponent(psr:diningRoom, psr:table) .
dul:hasComponent(psr:diningRoom, psr:chair1) .
dul:hasComponent(psr:diningRoom, psr:chair2) .
dul:hasComponent(psr:diningRoom, psr:chair3) .
geo:sfContains(psr:kitchen, psr:cabinet) .
geo:sfContains(psr:kitchen, psr:cupboard) .
geo:sfContains(psr:kitchen, psr:refrigerator) .