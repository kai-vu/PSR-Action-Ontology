@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix psr: <https://w3id.org/psr-task#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .

psr:kitchen a psr:Environment ;
  rdfs:label "kitchen"@en .

psr:diningArea a psr:Environment ;
  rdfs:label "dining area"@en .

psr:kitchenCabinet a psr:Component, psr:Furniture ;
  rdfs:label "kitchen cabinet"@en ;
  dul:hasLocation psr:kitchen .

psr:kitchenCountertop a psr:Component, psr:Location ;
  rdfs:label "kitchen countertop"@en ;
  dul:hasLocation psr:kitchen ;
  geo:sfWithin psr:kitchen .

psr:sink a psr:Component, psr:Appliance ;
  rdfs:label "sink"@en ;
  dul:hasLocation psr:kitchenCountertop .

psr:refrigerator a psr:Component, psr:Appliance ;
  rdfs:label "refrigerator"@en ;
  dul:hasLocation psr:kitchen .

psr:pot a psr:Component, psr:Object ;
  rdfs:label "pot"@en ;
  dul:hasLocation psr:kitchenCountertop .

psr:jarOfJam a psr:Component, psr:Object ;
  rdfs:label "jar of jam"@en ;
  dul:hasLocation psr:kitchenCountertop .

psr:boxOfRiceBubbles a psr:Component, psr:Object ;
  rdfs:label "box of Rice Bubbles"@en ;
  dul:hasLocation psr:kitchenCountertop .

psr:boxOfJello a psr:Component, psr:Object ;
  rdfs:label "box of Jello"@en ;
  dul:hasLocation psr:kitchenCountertop .

psr:boxOfShortbread a psr:Component, psr:Object ;
  rdfs:label "box of shortbread"@en ;
  dul:hasLocation psr:kitchenCountertop .

psr:plate a psr:Component, psr:Object ;
  rdfs:label "plate"@en ;
  dul:hasLocation psr:kitchenCountertop .

psr:diningTable a psr:Component, psr:Furniture ;
  rdfs:label "dining table"@en ;
  dul:hasLocation psr:diningArea .

psr:chair a psr:Component, psr:Furniture ;
  rdfs:label "chair"@en ;
  dul:hasLocation psr:diningArea .

psr:bottleOfWater a psr:Component, psr:Object ;
  rdfs:label "bottle of water"@en ;
  dul:hasLocation psr:diningTable .

psr:bowlOfFruit a psr:Component, psr:Object ;
  rdfs:label "bowl of fruit"@en ;
  dul:hasLocation psr:diningTable .

psr:book a psr:Component, psr:Object ;
  rdfs:label "book"@en ;
  dul:hasLocation psr:diningTable .

psr:floor a psr:Component, psr:Location ;
  rdfs:label "floor"@en ;
  dul:hasLocation psr:kitchen .

psr:wall a psr:Component, psr:Location ;
  rdfs:label "wall"@en ;
  dul:hasLocation psr:kitchen .

psr:ceiling a psr:Component, psr:Location ;
  rdfs:label "ceiling"@en ;
  dul:hasLocation psr:kitchen .

psr:lightFixture a psr:Component, psr:Object ;
  rdfs:label "light fixture"@en ;
  dul:hasLocation psr:ceiling .