@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix psr: <https://w3id.org/psr-task#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .

psr:Kitchen a psr:Environment ;
  rdfs:label "Kitchen"@en .

psr:DiningRoom a psr:Environment ;
  rdfs:label "Dining Room"@en .

psr:Kitchen_Dining_Room a psr:Environment ;
  rdfs:label "Kitchen and Dining Room"@en .

dul:hasComponent psr:Kitchen_Dining_Room psr:Kitchen .
dul:hasComponent psr:Kitchen_Dining_Room psr:DiningRoom .

psr:Dark_Cabinets a psr:Component ;
  rdfs:label "Dark Cabinets"@en .
psr:Tan_Marble_Countertop a psr:Component ;
  rdfs:label "Tan Marble Countertop"@en .
psr:White_Refrigerator a psr:Component ;
  rdfs:label "White Refrigerator"@en .
psr:Vent_Hood a psr:Component ;
  rdfs:label "Vent Hood"@en .
psr:Stovetop a psr:Component ;
  rdfs:label "Stovetop"@en .
psr:Pot a psr:Component ;
  rdfs:label "Pot"@en .
psr:Rice_Bubbles_Cereal a psr:Component ;
  rdfs:label "Rice Bubbles Cereal"@en .
psr:Strawberry_Banana_Jello a psr:Component ;
  rdfs:label "Strawberry Banana Jello"@en .
psr:Jam a psr:Component ;
  rdfs:label "Jam"@en .
psr:Shortbread_Cookies a psr:Component ;
  rdfs:label "Shortbread Cookies"@en .
psr:Yellow_Mug a psr:Component ;
  rdfs:label "Yellow Mug"@en .
psr:Box_of_Alpen_Salt a psr:Component ;
  rdfs:label "Box of Alpen Salt"@en .

psr:Wooden_Table a psr:Component ;
  rdfs:label "Wooden Table"@en .
psr:White_Chairs a psr:Component ;
  rdfs:label "White Chairs"@en .
psr:Bottle_of_Water a psr:Component ;
  rdfs:label "Bottle of Water"@en .
psr:Plate_with_Fruit a psr:Component ;
  rdfs:label "Plate with Fruit"@en .
psr:Cookbook a psr:Component ;
  rdfs:label "Cookbook"@en .
psr:Chucky_Puzzle a psr:Component ;
  rdfs:label "Chucky Puzzle"@en .

psr:Kitchen_Countertop_Location a psr:CurrentLocation ;
  rdfs:label "Kitchen Countertop Location"@en .
psr:Stovetop_Location a psr:CurrentLocation ;
  rdfs:label "Stovetop Location"@en .
psr:Dining_Table_Location a psr:CurrentLocation ;
  rdfs:label "Dining Table Location"@en .

dul:hasLocation psr:Dark_Cabinets psr:Kitchen_Countertop_Location .
dul:hasLocation psr:Tan_Marble_Countertop psr:Kitchen_Countertop_Location .
dul:hasLocation psr:White_Refrigerator psr:Kitchen_Countertop_Location .
dul:hasLocation psr:Vent_Hood psr:Kitchen_Countertop_Location .
dul:hasLocation psr:Stovetop psr:Stovetop_Location .
dul:hasLocation psr:Pot psr:Stovetop_Location .
dul:hasLocation psr:Rice_Bubbles_Cereal psr:Stovetop_Location .
dul:hasLocation psr:Yellow_Mug psr:Stovetop_Location .
dul:hasLocation psr:Strawberry_Banana_Jello psr:Kitchen_Countertop_Location .
dul:hasLocation psr:Jam psr:Kitchen_Countertop_Location .
dul:hasLocation psr:Shortbread_Cookies psr:Kitchen_Countertop_Location .
dul:hasLocation psr:Box_of_Alpen_Salt psr:Kitchen_Countertop_Location .
dul:hasLocation psr:Wooden_Table psr:Dining_Table_Location .
dul:hasLocation psr:White_Chairs psr:Dining_Table_Location .
dul:hasLocation psr:Bottle_of_Water psr:Dining_Table_Location .
dul:hasLocation psr:Plate_with_Fruit psr:Dining_Table_Location .
dul:hasLocation psr:Cookbook psr:Kitchen_Countertop_Location .
dul:hasLocation psr:Cookbook psr:Dining_Table_Location .
dul:hasLocation psr:Chucky_Puzzle psr:Dining_Table_Location .

geo:sfWithin psr:Kitchen_Countertop_Location psr:Kitchen .
geo:sfWithin psr:Stovetop_Location psr:Kitchen .
geo:sfWithin psr:Dining_Table_Location psr:DiningRoom .

psr:Kitchen rdfs:comment "The kitchen is part of the kitchen and dining room environment."@en .
psr:DiningRoom rdfs:comment "The dining room is part of the kitchen and dining room environment."@en .

geo:sfOverlaps psr:Kitchen_Countertop_Location psr:Kitchen .
geo:sfOverlaps psr:Stovetop_Location psr:Kitchen .
geo:sfOverlaps psr:Dining_Table_Location psr:DiningRoom .