@prefix kitchen: <http://example.org/kitchen#> .

kitchen:dining_room kitchen:hasComponent kitchen:door,
        kitchen:round_light_fixture,
        kitchen:wooden_table .

kitchen:environment kitchen:hasComponent kitchen:white_walls,
        kitchen:wooden_floors ;
    kitchen:isA kitchen:kitchen_and_dining_room .

kitchen:kitchen kitchen:hasComponent kitchen:dark_gray_cabinet,
        kitchen:dark_gray_cabinets,
        kitchen:refrigerator,
        kitchen:sink,
        kitchen:stove .

kitchen:beige_countertop kitchen:onTopOf kitchen:box_of_strawberry_banana_Jello,
        kitchen:jar_of_jam,
        kitchen:package_of_Alpen_Salt,
        kitchen:white_plates .

kitchen:dark_gray_cabinet kitchen:hasComponent kitchen:beige_countertop .

kitchen:plate kitchen:onTopOf kitchen:bowl .

kitchen:round_light_fixture kitchen:sfWithin kitchen:ceiling .

kitchen:stove kitchen:onTopOf kitchen:pot .

kitchen:wooden_floors kitchen:sfContains kitchen:herringbone_pattern .

kitchen:wooden_table kitchen:hasComponent kitchen:white_chairs ;
    kitchen:onTopOf kitchen:book,
        kitchen:bottle_of_Evian_water,
        kitchen:plate .

