@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/psr-task#> .

psr:kitchen a psr:Environment ;
    rdfs:label "kitchen"@en .

psr:refrigerator a psr:Appliance ;
    rdfs:label "refrigerator"@en ;
    psr:hasLocation psr:kitchen .

psr:table a psr:Furniture ;
    rdfs:label "table"@en ;
    psr:hasLocation psr:kitchen .

psr:chair a psr:Furniture ;
    rdfs:label "chair"@en ;
    psr:hasLocation psr:kitchen .

psr:cup a psr:Object ;
    rdfs:label "cup"@en ;
    psr:hasLocation psr:table .

psr:book a psr:Object ;
    rdfs:label "book"@en ;
    psr:hasLocation psr:table .

psr:water_bottle a psr:Object ;
    rdfs:label "water bottle"@en ;
    psr:hasLocation psr:table .

psr:sink a psr:Component ;
    rdfs:label "sink"@en ;
    psr:hasLocation psr:kitchen .

psr:stove a psr:Appliance ;
    rdfs:label "stove"@en ;
    psr:hasLocation psr:kitchen .

psr:plate a psr:Object ;
    rdfs:label "plate"@en ;
    psr:hasLocation psr:kitchen .

psr:cabinet a psr:Furniture ;
    rdfs:label "cabinet"@en ;
    psr:hasLocation psr:kitchen .

psr:drawer a psr:Furniture ;
    rdfs:label "drawer"@en ;
    psr:hasLocation psr:kitchen .

psr:countertop a psr:Component ;
    rdfs:label "countertop"@en ;
    psr:hasLocation psr:kitchen .

dul:hasComponent(psr:kitchen, psr:refrigerator) .
dul:hasComponent(psr:kitchen, psr:table) .
dul:hasComponent(psr:kitchen, psr:chair) .
dul:hasComponent(psr:kitchen, psr:cabinet) .
dul:hasComponent(psr:kitchen, psr:drawer) .
dul:hasComponent(psr:kitchen, psr:countertop) .
dul:hasComponent(psr:kitchen, psr:sink) .
dul:hasComponent(psr:kitchen, psr:stove) .

psr:hasAffordance(psr:cup, soma:PickingUp) .
psr:hasAffordance(psr:cup, soma:PuttingDown) .
psr:hasAffordance(psr:book, soma:PickingUp) .
psr:hasAffordance(psr:book, soma:PuttingDown) .
psr:hasAffordance(psr:water_bottle, soma:PickingUp) .
psr:hasAffordance(psr:water_bottle, soma:PuttingDown) .
psr:hasAffordance(psr:plate, soma:PickingUp) .
psr:hasAffordance(psr:plate, soma:PuttingDown) .

geo:sfWithin(psr:refrigerator, psr:kitchen) .
geo:sfWithin(psr:table, psr:kitchen) .
geo:sfWithin(psr:chair, psr:kitchen) .
geo:sfWithin(psr:cabinet, psr:kitchen) .
geo:sfWithin(psr:drawer, psr:kitchen) .
geo:sfWithin(psr:countertop, psr:kitchen) .
geo:sfWithin(psr:sink, psr:kitchen) .
geo:sfWithin(psr:stove, psr:kitchen) .