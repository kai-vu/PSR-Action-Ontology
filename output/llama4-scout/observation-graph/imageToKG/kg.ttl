@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/psr-task#> .

psr:kitchen a psr:Environment ;
    rdfs:label "kitchen"@en .

psr:refrigerator a psr:Appliance ;
    rdfs:label "refrigerator"@en ;
    psr:hasLocation psr:kitchen .

psr:table a psr:Furniture ;
    rdfs:label "table"@en ;
    psr:hasLocation psr:kitchen .

psr:chair a psr:Furniture ;
    rdfs:label "chair"@en ;
    psr:hasLocation psr:kitchen .

psr:cupboard a psr:Furniture ;
    rdfs:label "cupboard"@en ;
    psr:hasLocation psr:kitchen .

psr:water_bottle a psr:Object ;
    rdfs:label "water bottle"@en ;
    psr:hasLocation psr:table .

psr:book a psr:Object ;
    rdfs:label "book"@en ;
    psr:hasLocation psr:table .

psr:jello_box a psr:Object ;
    rdfs:label "Jello box"@en ;
    psr:hasLocation psr:cupboard .

psr:plates a psr:Object ;
    rdfs:label "plates"@en ;
    psr:hasLocation psr:cupboard .

psr:cups a psr:Object ;
    rdfs:label "cups"@en ;
    psr:hasLocation psr:cupboard .

psr:jar a psr:Object ;
    rdfs:label "jar"@en ;
    psr:hasLocation psr:cupboard .

psr:alpen_salt a psr:Object ;
    rdfs:label "Alpen Salt"@en ;
    psr:hasLocation psr:cupboard .

psr:evian_water a psr:Object ;
    rdfs:label "Evian water"@en ;
    psr:hasLocation psr:cupboard .

dul:hasComponent(psr:kitchen, psr:refrigerator) ;
    dul:hasComponent(psr:kitchen, psr:table) ;
    dul:hasComponent(psr:kitchen, psr:chair) ;
    dul:hasComponent(psr:kitchen, psr:cupboard) .

dul:hasComponent(psr:cupboard, psr:jello_box) ;
    dul:hasComponent(psr:cupboard, psr:plates) ;
    dul:hasComponent(psr:cupboard, psr:cups) ;
    dul:hasComponent(psr:cupboard, psr:jar) ;
    dul:hasComponent(psr:cupboard, psr:alpen_salt) ;
    dul:hasComponent(psr:cupboard, psr:evian_water) .

psr:onTopOf(psr:refrigerator, psr:kitchen) ;
    psr:onTopOf(psr:table, psr:kitchen) ;
    psr:onTopOf(psr:chair, psr:kitchen) ;
    psr:onTopOf(psr:cupboard, psr:kitchen) ;

geo:sfContains(psr:kitchen, psr:refrigerator) ;
    geo:sfContains(psr:kitchen, psr:table) ;
    geo:sfContains(psr:kitchen, psr:chair) ;
    geo:sfContains(psr:kitchen, psr:cupboard) ;