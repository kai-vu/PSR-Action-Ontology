@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/psr-task#> .

psr:KitchenAndDiningRoom a psr:Environment ;
  rdfs:label "Kitchen and Dining Room"@en .

psr:Kitchen a psr:Component, psr:Environment ;
  rdfs:label "Kitchen"@en ;
  dul:hasLocation psr:KitchenLocation .

psr:KitchenLocation a psr:Location ;
  rdfs:label "Kitchen Location"@en .

psr:DiningRoom a psr:Component, psr:Environment ;
  rdfs:label "Dining Room"@en ;
  dul:hasLocation psr:DiningRoomLocation .

psr:DiningRoomLocation a psr:Location ;
  rdfs:label "Dining Room Location"@en .

psr:DarkCabinets a psr:Component ;
  rdfs:label "Dark Cabinets"@en ;
  dul:hasLocation psr:KitchenCabinetsLocation .

psr:KitchenCabinetsLocation a psr:Location ;
  rdfs:label "Kitchen Cabinets Location"@en ;
  geo:sfWithin psr:KitchenLocation .

psr:TanMarbleCountertop a psr:Component ;
  rdfs:label "Tan Marble Countertop"@en ;
  dul:hasLocation psr:KitchenCountertopLocation .

psr:KitchenCountertopLocation a psr:Location ;
  rdfs:label "Kitchen Countertop Location"@en ;
  geo:sfWithin psr:KitchenLocation .

psr:WhiteRefrigerator a psr:Appliance ;
  rdfs:label "White Refrigerator"@en ;
  dul:hasLocation psr:KitchenRefrigeratorLocation .

psr:KitchenRefrigeratorLocation a psr:Location ;
  rdfs:label "Kitchen Refrigerator Location"@en ;
  geo:sfWithin psr:KitchenLocation .

psr:RiceBubblesCereal a psr:Object ;
  rdfs:label "Rice Bubbles Cereal"@en ;
  dul:hasLocation psr:KitchenCountertopLocation .

psr:StrawberryBananaJello a psr:Object ;
  rdfs:label "Strawberry Banana Jello"@en ;
  dul:hasLocation psr:KitchenCountertopLocation .

psr:JarOfJam a psr:Object ;
  rdfs:label "Jar of Jam"@en ;
  dul:hasLocation psr:KitchenCountertopLocation .

psr:ShortbreadCookies a psr:Object ;
  rdfs:label "Shortbread Cookies"@en ;
  dul:hasLocation psr:KitchenCountertopLocation .

psr:YellowMug a psr:Object ;
  rdfs:label "Yellow Mug"@en ;
  dul:hasLocation psr:NearStovetopLocation .

psr:NearStovetopLocation a psr:Location ;
  rdfs:label "Near Stovetop Location"@en ;
  geo:sfWithin psr:KitchenLocation .

psr:Pot a psr:Object ;
  rdfs:label "Pot"@en ;
  dul:hasLocation psr:OnStovetopLocation .

psr:OnStovetopLocation a psr:Location ;
  rdfs:label "On Stovetop Location"@en ;
  geo:sfWithin psr:KitchenLocation .

psr:VentHood a psr:Appliance ;
  rdfs:label "Vent Hood"@en ;
  dul:hasLocation psr:AboveCountertopLocation .

psr:AboveCountertopLocation a psr:Location ;
  rdfs:label "Above Countertop Location"@en ;
  geo:sfWithin psr:KitchenLocation .

psr:WoodenTable a psr:Furniture ;
  rdfs:label "Wooden Table"@en ;
  dul:hasLocation psr:DiningTableLocation .

psr:DiningTableLocation a psr:Location ;
  rdfs:label "Dining Table Location"@en ;
  geo:sfWithin psr:DiningRoomLocation .

psr:WhiteChairs a psr:Furniture ;
  rdfs:label "White Chairs"@en ;
  dul:hasLocation psr:NearDiningTableLocation .

psr:NearDiningTableLocation a psr:Location ;
  rdfs:label "Near Dining Table Location"@en ;
  geo:sfWithin psr:DiningRoomLocation .

psr:BottleOfWater a psr:Object ;
  rdfs:label "Bottle of Water"@en ;
  dul:hasLocation psr:DiningTableLocation .

psr:PlateWithFruit a psr:Object ;
  rdfs:label "Plate with Fruit"@en ;
  dul:hasLocation psr:DiningTableLocation .

psr:Cookbook a psr:Object ;
  rdfs:label "Cookbook"@en ;
  dul:hasLocation psr:KitchenCountertopLocation .

psr:ChuckyPuzzle a psr:Object ;
  rdfs:label "Chucky Puzzle"@en ;
  dul:hasLocation psr:DiningTableLocation .

psr:KitchenAndDiningRoom dul:hasComponent psr:Kitchen .
psr:KitchenAndDiningRoom dul:hasComponent psr:DiningRoom .

psr:Kitchen dul:hasComponent psr:DarkCabinets .
psr:Kitchen dul:hasComponent psr:TanMarbleCountertop .
psr:Kitchen dul:hasComponent psr:WhiteRefrigerator .

