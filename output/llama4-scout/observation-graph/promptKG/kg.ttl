@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/psr-task#> .

psr:KitchenAndDiningArea a psr:Environment ;
  dul:hasComponent psr:Kitchen ;
  dul:hasComponent psr:DiningArea .

psr:Kitchen a psr:Component ;
  psr:hasAffordance soma:Opening ;
  dul:hasLocation psr:KitchenLocation ;
  geo:sfContains psr:Cabinets ;
  geo:sfContains psr:Countertops ;
  geo:sfContains psr:Sink ;
  geo:sfContains psr:Refrigerator .

psr:KitchenLocation a psr:Location .

psr:Cabinets a psr:Component ;
  dul:hasLocation psr:CabinetsLocation .

psr:CabinetsLocation a psr:Location ;
  psr:onTopOf psr:KitchenLocation .

psr:Countertops a psr:Component ;
  dul:hasLocation psr:CountertopsLocation .

psr:CountertopsLocation a psr:Location ;
  psr:onTopOf psr:KitchenLocation .

psr:Sink a psr:Appliance ;
  dul:hasLocation psr:SinkLocation .

psr:SinkLocation a psr:Location ;
  psr:onTopOf psr:KitchenLocation .

psr:Refrigerator a psr:Appliance ;
  dul:hasLocation psr:RefrigeratorLocation .

psr:RefrigeratorLocation a psr:Location ;
  psr:onTopOf psr:KitchenLocation .

psr:Pot a psr:Object ;
  dul:hasLocation psr:PotLocation .

psr:PotLocation a psr:Location ;
  psr:onTopOf psr:CountertopsLocation .

psr:JarOfJam a psr:Object ;
  dul:hasLocation psr:jarOfJamLocation .

psr:jarOfJamLocation a psr:Location ;
  psr:onTopOf psr:CountertopsLocation .

psr:BoxOfRiceBubbles a psr:Object ;
  dul:hasLocation psr:boxOfRiceBubblesLocation .

psr:boxOfRiceBubblesLocation a psr:Location ;
  psr:onTopOf psr:CountertopsLocation .

psr:BoxOfJello a psr:Object ;
  dul:hasLocation psr:boxOfJelloLocation .

psr:boxOfJelloLocation a psr:Location ;
  psr:onTopOf psr:CountertopsLocation .

psr:BoxOfShortbread a psr:Object ;
  dul:hasLocation psr:boxOfShortbreadLocation .

psr:boxOfShortbreadLocation a psr:Location ;
  psr:onTopOf psr:CountertopsLocation .

psr:Plate a psr:Object ;
  dul:hasLocation psr:plateLocation .

psr:plateLocation a psr:Location ;
  psr:onTopOf psr:CountertopsLocation .

psr:DiningArea a psr:Component ;
  psr:hasAffordance soma:Opening ;
  dul:hasLocation psr:DiningAreaLocation ;
  geo:sfContains psr:Table ;
  geo:sfContains psr:Chairs .

psr:DiningAreaLocation a psr:Location .

psr:Table a psr:Furniture ;
  dul:hasLocation psr:TableLocation .

psr:TableLocation a psr:Location ;
  psr:onTopOf psr:DiningAreaLocation .

psr:Chairs a psr:Furniture ;
  dul:hasLocation psr:ChairsLocation .

psr:ChairsLocation a psr:Location ;
  psr:onTopOf psr:DiningAreaLocation .

psr:BottleOfWater a psr:Object ;
  dul:hasLocation psr:bottleOfWaterLocation .

psr:bottleOfWaterLocation a psr:Location ;
  psr:onTopOf psr:TableLocation .

psr:BowlOfFruit a psr:Object ;
  dul:hasLocation psr:bowlOfFruitLocation .

psr:bowlOfFruitLocation a psr:Location ;
  psr:onTopOf psr:TableLocation .

psr:Book a psr:Object ;
  dul:hasLocation psr:bookLocation .

psr:bookLocation a psr:Location ;
  psr:onTopOf psr:TableLocation .