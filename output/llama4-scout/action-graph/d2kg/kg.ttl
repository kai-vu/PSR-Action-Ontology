@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/psr-task#> .

psr:RestoreKitchen a psr:Instruction ;
  psr:hasNaturalLanguage "Restore the kitchen to an organized state by identifying all misplaced items and returning them to their standard storage locations based on their type and function."@en .

psr:RestoreKitchenWorkflow a psr:Workflow ;
  psr:hasAction psr:IdentifyMisplacedItems ;
  psr:hasAction psr:ReturnItemsToStorage .

psr:IdentifyMisplacedItems a dul:Action ;
  soma:isPerformedBy psr:Robot ;
  psr:actsOn psr:Kitchen ;
  psr:isAffordedBy soma:PickingUp .

psr:ReturnItemsToStorage a dul:Action ;
  soma:isPerformedBy psr:Robot ;
  psr:actsOn psr:MisplacedItem ;
  psr:isAffordedBy soma:PuttingDown .

psr:Robot a dul:Agent .

psr:Kitchen a psr:Component .

psr:MisplacedItem a psr:Object .

psr:EvianWater a psr:Object ;
  psr:actsOn psr:EvianWater .

psr:Book a psr:Object ;
  psr:actsOn psr:Book .

psr:Plate a psr:Object ;
  psr:actsOn psr:Plate .

psr:Cabinet a psr:Component .

psr:CounterTop a psr:Component .

psr:RestoreKitchen psr:hasWorkflow psr:RestoreKitchenWorkflow .

psr:IdentifyMisplacedItems psr:precedes psr:ReturnItemsToStorage .

psr:ReturnItemsToStorage psr:follows psr:IdentifyMisplacedItems .

psr:EvianWater psr:isAffordedBy soma:PickingUp .

psr:EvianWater psr:isAffordedBy soma:PuttingDown .

psr:Book psr:isAffordedBy soma:PickingUp .

psr:Book psr:isAffordedBy soma:PuttingDown .

psr:Plate psr:isAffordedBy soma:PickingUp .

psr:Plate psr:isAffordedBy soma:PuttingDown .