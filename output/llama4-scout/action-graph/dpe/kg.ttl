@prefix kitchen: <http://example.org/kitchen#> .

kitchen:countertop kitchen:hasObject kitchen:stack_of_white_plates .

kitchen:dining_room kitchen:hasObject kitchen:table .

kitchen:kitchen kitchen:hasObject kitchen:cabinet,
        kitchen:countertop,
        kitchen:refrigerator .

kitchen:move_to_dining_room kitchen:isPerformedBy kitchen:assistant .

kitchen:table kitchen:hasObject kitchen:plate .

kitchen:workflow kitchen:hasAction kitchen:move_to_dining_room .

kitchen:Evian_water kitchen:isPickedUpBy kitchen:assistant ;
    kitchen:isPutDownBy kitchen:assistant .

kitchen:book kitchen:isPickedUpBy kitchen:assistant ;
    kitchen:isPutDownBy kitchen:assistant .

kitchen:bowl kitchen:isPickedUpBy kitchen:assistant ;
    kitchen:isPutDownBy kitchen:assistant .

kitchen:box_of_strawberry_banana_Jello kitchen:isPickedUpBy kitchen:assistant ;
    kitchen:isPutDownBy kitchen:assistant .

kitchen:jar_of_jam kitchen:isPickedUpBy kitchen:assistant ;
    kitchen:isPutDownBy kitchen:assistant .

kitchen:package_of_Alpen_Salt kitchen:isPickedUpBy kitchen:assistant ;
    kitchen:isPutDownBy kitchen:assistant .

kitchen:plate kitchen:isPickedUpBy kitchen:assistant ;
    kitchen:isPutDownBy kitchen:assistant .

kitchen:assistant kitchen:hasWorkflow kitchen:workflow ;
    kitchen:movesTo kitchen:cabinet,
        kitchen:dining_room,
        kitchen:kitchen ;
    kitchen:picksUp kitchen:Evian_water,
        kitchen:book,
        kitchen:bowl,
        kitchen:box_of_strawberry_banana_Jello,
        kitchen:jar_of_jam,
        kitchen:package_of_Alpen_Salt,
        kitchen:plate ;
    kitchen:putsDown kitchen:Evian_water,
        kitchen:book,
        kitchen:bowl,
        kitchen:box_of_strawberry_banana_Jello,
        kitchen:jar_of_jam,
        kitchen:package_of_Alpen_Salt,
        kitchen:plate .

