@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/psr-task#> .

# Create instruction
psr:RestoreKitchen a psr:Instruction ;
  psr:hasNaturalLanguage "Restore the kitchen to an organized state by identifying all misplaced items and returning them to their standard storage locations based on their type and function."@en .

# Create workflow
psr:RestoreKitchenWorkflow a psr:Workflow ;
  psr:hasAction psr:IdentifyMisplacedItems ;
  psr:hasAction psr:ReturnItemsToStorage .

# Define actions
psr:IdentifyMisplacedItems a dul:Action ;
  psr:isPerformedBy psr:Robot ;
  psr:actsOn psr:KitchenCounter ;
  psr:isAffordedBy soma:Identifying .

psr:ReturnItemsToStorage a dul:Action ;
  psr:isPerformedBy psr:Robot ;
  psr:actsOn psr:MisplacedItems ;
  psr:isAffordedBy soma:Returning .

psr:PickUpItem a dul:Action ;
  psr:isPerformedBy psr:Robot ;
  psr:actsOn psr:MisplacedItems ;
  psr:isAffordedBy soma:PickingUp .

psr:PutDownItem a dul:Action ;
  psr:isPerformedBy psr:Robot ;
  psr:actsOn psr:StorageLocations ;
  psr:isAffordedBy soma:PuttingDown .

# Define components
psr:KitchenCounter a psr:Component ;
  rdfs:label "Kitchen Counter"@en .

psr:MisplacedItems a psr:Object ;
  rdfs:label "Misplaced Items"@en .

psr:StorageLocations a psr:Component ;
  rdfs:label "Storage Locations"@en .

psr:Robot a dul:Agent ;
  rdfs:label "Robot"@en .

# Define affordances
soma:Identifying a soma:Affordance ;
  rdfs:label "Identifying"@en .

soma:Returning a soma:Affordance ;
  rdfs:label "Returning"@en .

# Relationships between actions
psr:IdentifyMisplacedItems psr:precedes psr:ReturnItemsToStorage .
psr:ReturnItemsToStorage psr:follows psr:IdentifyMisplacedItems .

# Workflow and instruction relationship
psr:RestoreKitchen psr:hasWorkflow psr:RestoreKitchenWorkflow . 

# Specify items on the counter
psr:Pot a psr:Object ;
  rdfs:label "Pot"@en .

psr:JarOfJam a psr:Object ;
  rdfs:label "Jar of Jam"@en .

psr:BoxOfRiceBubbles a psr:Object ;
  rdfs:label "Box of Rice Bubbles"@en .

psr:BoxOfJello a psr:Object ;
  rdfs:label "Box of Jello"@en .

psr:BoxOfShortbread a psr:Object ;
  rdfs:label "Box of Shortbread"@en .

psr:Plate a psr:Object ;
  rdfs:label "Plate"@en .

psr:BottleOfWater a psr:Object ;
  rdfs:label "Bottle of Water"@en .

psr:BowlOfFruit a psr:Object ;
  rdfs:label "Bowl of Fruit"@en .

psr:Book a psr:Object ;
  rdfs:label "Book"@en .

psr:KitchenCabinets a psr:Component ;
  rdfs:label "Kitchen Cabinets"@en .

# Specify storage locations
psr:KitchenCabinets psr:hasAffordance soma:Closing .

# Misplaced items and their storage locations
psr:Pot psr:actsOn psr:KitchenCabinets .
psr:JarOfJam psr:actsOn psr:KitchenCabinets .
psr:BoxOfRiceBubbles psr:actsOn psr:KitchenCabinets .
psr:BoxOfJello psr:actsOn psr:KitchenCabinets .
psr:BoxOfShortbread psr:actsOn psr:KitchenCabinets .
psr:Plate psr:actsOn psr:KitchenCabinets .

# Specify actions for each item
psr:PickUpItem1 a psr:PickUpItem ;
  psr:actsOn psr:Pot .

psr:PutDownItem1 a psr:PutDownItem ;
  psr:actsOn psr:KitchenCabinets .

psr:PickUpItem2 a psr:PickUpItem ;
  psr:actsOn psr:JarOfJam .

psr:PutDownItem2 a psr:PutDownItem ;
  psr:actsOn psr:KitchenCabinets .

psr:PickUpItem3 a psr:PickUpItem ;
  psr:actsOn psr:BoxOfRiceBubbles .

psr:PutDownItem3 a psr:PutDownItem ;
  psr:actsOn psr:KitchenCabinets .

psr:PickUpItem4 a psr:PickUpItem ;
  psr:actsOn psr:BoxOfJello .

psr:PutDownItem4 a psr:PutDownItem ;
  psr:actsOn psr:KitchenCabinets .

psr:PickUpItem5 a psr:PickUpItem ;
  psr:actsOn psr:BoxOfShortbread .

psr:PutDownItem5 a psr:PutDownItem ;
  psr:actsOn psr:KitchenCabinets .

psr:PickUpItem6 a psr:PickUpItem ;
  psr:actsOn psr:Plate .

psr:PutDownItem6 a psr:PutDownItem ;
  psr:actsOn psr:KitchenCabinets .

# Sequence actions
psr:IdentifyMisplacedItems psr:precedes psr:PickUpItem1 .
psr:PickUpItem1 psr:follows psr:IdentifyMisplacedItems .
psr:PickUpItem1 psr:precedes psr:PutDownItem1 .
psr:PutDownItem1 psr:follows psr:PickUpItem1 .

psr:PutDownItem1 psr:precedes psr:PickUpItem2 .
psr:PickUpItem2 psr:follows psr:PutDownItem1 .
psr:PickUpItem2 psr:precedes psr:PutDownItem2 .
psr:PutDownItem2 psr:follows psr:PickUpItem2 .

psr:PutDownItem2 psr:precedes psr:PickUpItem3 .
psr:PickUpItem3 psr:follows psr:PutDownItem2 .
psr:PickUpItem3 psr:precedes psr:PutDownItem3 .
psr:PutDownItem3 psr:follows psr:PickUpItem3 .

psr:PutDownItem3 psr:precedes psr:PickUpItem4 .
psr:PickUpItem4 psr:follows psr:PutDownItem3 .
psr:PickUpItem4 psr:precedes psr:PutDownItem4 .
psr:PutDownItem4 psr:follows psr:PickUpItem4 .

psr:PutDownItem4 psr:precedes psr:PickUpItem5 .
psr:PickUpItem5 psr:follows psr:PutDownItem4 .
psr:PickUpItem5 psr:precedes psr:PutDownItem5 .
psr:PutDownItem5 psr:follows psr:PickUpItem5 .

psr:PutDownItem5 psr:precedes psr:PickUpItem6 .
psr:PickUpItem6 psr:follows psr:PutDownItem5 .
psr:PickUpItem6 psr:precedes psr:PutDownItem6 .
psr:PutDownItem6 psr:follows psr:PickUpItem6 .

psr:PutDownItem6 psr:precedes psr:ReturnItemsToStorage .
psr:ReturnItemsToStorage psr:follows psr:PutDownItem6 .