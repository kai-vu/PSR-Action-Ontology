@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/psr-task#> .

### Instruction
psr:RestoreKitchen a psr:Instruction ;
  psr:hasNaturalLanguage "Restore the kitchen to an organized state by identifying all misplaced items and returning them to their standard storage locations based on their type and function."@en .

### Workflows
psr:RestoreKitchenWorkflow a psr:Workflow ;
  psr:hasAction psr:IdentifyMisplacedItems ;
  psr:hasAction psr:ReturnItemsToStorage .

### Actions
psr:IdentifyMisplacedItems a dul:Action ;
  psr:isPerformedBy psr:Robot ;
  psr:actsOn psr:Kitchen .

psr:ReturnItemsToStorage a dul:Action ;
  psr:isPerformedBy psr:Robot ;
  psr:actsOn psr:Kitchen .

### Agent
psr:Robot a dul:Agent .

### Components
psr:Kitchen a psr:Component .

psr:RiceBubblesCereal a psr:Object ;
  psr:isAffordedBy soma:PickingUp .

psr:StrawberryBananaJello a psr:Object ;
  psr:isAffordedBy soma:PickingUp .

psr:Jam a psr:Object ;
  psr:isAffordedBy soma:PickingUp .

psr:ShortbreadCookies a psr:Object ;
  psr:isAffordedBy soma:PickingUp .

psr:YellowMug a psr:Object ;
  psr:isAffordedBy soma:PickingUp .

psr:BottleOfWater a psr:Object ;
  psr:isAffordedBy soma:PickingUp .

psr:PlateWithFruit a psr:Object ;
  psr:isAffordedBy soma:PickingUp .

psr:Cookbook a psr:Object ;
  psr:isAffordedBy soma:PickingUp .

### Affordances
psr:PickUpRiceBubblesCereal a soma:PickingUp ;
  psr:actsOn psr:RiceBubblesCereal .

psr:PickUpStrawberryBananaJello a soma:PickingUp ;
  psr:actsOn psr:StrawberryBananaJello .

psr:PickUpJam a soma:PickingUp ;
  psr:actsOn psr:Jam .

psr:PickUpShortbreadCookies a soma:PickingUp ;
  psr:actsOn psr:ShortbreadCookies .

psr:PickUpYellowMug a soma:PickingUp ;
  psr:actsOn psr:YellowMug .

psr:PickUpBottleOfWater a soma:PickingUp ;
  psr:actsOn psr:BottleOfWater .

psr:PickUpPlateWithFruit a soma:PickingUp ;
  psr:actsOn psr:PlateWithFruit .

psr:PickUpCookbook a soma:PickingUp ;
  psr:actsOn psr:Cookbook .

### Relations
psr:RestoreKitchen psr:hasWorkflow psr:RestoreKitchenWorkflow .

psr:RestoreKitchenWorkflow psr:hasAction psr:IdentifyMisplacedItems ;
psr:RestoreKitchenWorkflow psr:hasAction psr:ReturnItemsToStorage .

psr:IdentifyMisplacedItems psr:precedes psr:ReturnItemsToStorage .

psr:IdentifyMisplacedItems psr:isPerformedBy psr:Robot .

psr:ReturnItemsToStorage psr:isPerformedBy psr:Robot .

psr:ReturnItemsToStorage psr:actsOn psr:RiceBubblesCereal ;
psr:ReturnItemsToStorage psr:actsOn psr:StrawberryBananaJello ;
psr:ReturnItemsToStorage psr:actsOn psr:Jam ;
psr:ReturnItemsToStorage psr:actsOn psr:ShortbreadCookies ;
psr:ReturnItemsToStorage psr:actsOn psr:YellowMug ;
psr:ReturnItemsToStorage psr:actsOn psr:BottleOfWater ;
psr:ReturnItemsToStorage psr:actsOn psr:PlateWithFruit ;
psr:ReturnItemsToStorage psr:actsOn psr:Cookbook .

### Putting down items
psr:PuttingDownRiceBubblesCereal a soma:PuttingDown ;
  psr:actsOn psr:RiceBubblesCereal .

psr:PuttingDownStrawberryBananaJello a soma:PuttingDown ;
  psr:actsOn psr:StrawberryBananaJello .

psr:PuttingDownJam a soma:PuttingDown ;
  psr:actsOn psr:Jam .

psr:PuttingDownShortbreadCookies a soma:PuttingDown ;
  psr:actsOn psr:ShortbreadCookies .

psr:PuttingDownYellowMug a soma:PuttingDown ;
  psr:actsOn psr:YellowMug .

psr:PuttingDownBottleOfWater a soma:PuttingDown ;
  psr:actsOn psr:BottleOfWater .

psr:PuttingDownPlateWithFruit a soma:PuttingDown ;
  psr:actsOn psr:PlateWithFruit .

psr:PuttingDownCookbook a soma:PuttingDown ;
  psr:actsOn psr:Cookbook .

### Detailed action sequence
psr:PickUpRiceBubblesCereal psr:precedes psr:PuttingDownRiceBubblesCereal .
psr:PickUpStrawberryBananaJello psr:precedes psr:PuttingDownStrawberryBananaJello .
psr:PickUpJam psr:precedes psr:PuttingDownJam .
psr:PickUpShortbreadCookies psr:precedes psr:PuttingDownShortbreadCookies .
psr:PickUpYellowMug psr:precedes psr:PuttingDownYellowMug .
psr:PickUpBottleOfWater psr:precedes psr:PuttingDownBottleOfWater .
psr:PickUpPlateWithFruit psr:precedes psr:PuttingDownPlateWithFruit .
psr:PickUpCookbook psr:precedes psr:PuttingDownCookbook .

psr:IdentifyMisplacedItems psr:hasAction psr:PickUpRiceBubblesCereal .
psr:IdentifyMisplacedItems psr:hasAction psr:PickUpStrawberryBananaJello .
psr:IdentifyMisplacedItems psr:hasAction psr:PickUpJam .
psr:IdentifyMisplacedItems psr:hasAction psr:PickUpShortbreadCookies .
psr:IdentifyMisplacedItems psr:hasAction psr:PickUpYellowMug .
psr:IdentifyMisplacedItems psr:hasAction psr:PickUpBottleOfWater .
psr:IdentifyMisplacedItems psr:hasAction psr:PickUpPlateWithFruit .
psr:IdentifyMisplacedItems psr:hasAction psr:PickUpCookbook .

psr:ReturnItemsToStorage psr:hasAction psr:PuttingDownRiceBubblesCereal .
psr:ReturnItemsToStorage psr:hasAction psr:PuttingDownStrawberryBananaJello .
psr:ReturnItemsToStorage psr:hasAction psr:PuttingDownJam .
psr:ReturnItemsToStorage psr:hasAction psr:PuttingDownShortbreadCookies .
psr:ReturnItemsToStorage psr:hasAction psr:PuttingDownYellowMug .
psr:ReturnItemsToStorage psr:hasAction psr:PuttingDownBottleOfWater .
psr:ReturnItemsToStorage psr:hasAction psr:PuttingDownPlateWithFruit .
psr:ReturnItemsToStorage psr:hasAction psr:PuttingDownCookbook .