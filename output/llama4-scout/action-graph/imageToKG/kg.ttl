@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/psr-task#> .

### Instances
psr:inst1 a psr:Instruction ;
  psr:hasNaturalLanguage "Restore the kitchen to an organized state by identifying all misplaced items and returning them to their standard storage locations based on their type and function."@en .

psr:agent1 a dul:Agent .

### Actions and Workflow
psr:workflow1 a psr:Workflow ;
  psr:hasAction psr:action1, psr:action2, psr:action3, psr:action4, psr:action5 .

psr:action1 a dul:Action ;
  psr:isAffordedBy soma:PickingUp ;
  psr:actsOn psr:book1 .

psr:action2 a dul:Action ;
  psr:isAffordedBy soma:PuttingDown ;
  psr:actsOn psr:book1 .

psr:action3 a dul:Action ;
  psr:isAffordedBy soma:PickingUp ;
  psr:actsOn psr:bottle1 .

psr:action4 a dul:Action ;
  psr:isAffordedBy soma:PuttingDown ;
  psr:actsOn psr:bottle1 .

psr:action5 a dul:Action ;
  psr:isAffordedBy soma:Opening ;
  psr:actsOn psr:refrigerator1 .

### Objects
psr:book1 a psr:Object, psr:Component ;
  rdfs:label "Chucky Puzzle"@en .

psr:bottle1 a psr:Object, psr:Component ;
  rdfs:label "Water Bottle"@en .

psr:refrigerator1 a psr:Appliance, psr:Component ;
  rdfs:label "Refrigerator"@en .

### Relations
psr:action1 psr:precedes psr:action2 .
psr:action2 psr:follows psr:action1 .
psr:action2 psr:precedes psr:action3 .
psr:action3 psr:follows psr:action2 .
psr:action3 psr:precedes psr:action4 .
psr:action4 psr:follows psr:action3 .
psr:action4 psr:precedes psr:action5 .
psr:action5 psr:follows psr:action4 .

psr:inst1 psr:hasWorkflow psr:workflow1 .
psr:agent1 a dul:Agent ;
  psr:isPerformedBy psr:action1, psr:action2, psr:action3, psr:action4 .