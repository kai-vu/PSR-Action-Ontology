@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/psr-task#> .

psr:RestoreKitchenToOrganizedState a psr:Instruction ;
    psr:hasNaturalLanguage "Restore the kitchen to an organized state by identifying all misplaced items and returning them to their standard storage locations based on their type and function."@en ;
    psr:hasWorkflow psr:Workflow_RestoreKitchen .

psr:Workflow_RestoreKitchen a psr:Workflow ;
    psr:hasAction dul:Action1, dul:Action2, dul:Action3, dul:Action4, dul:Action5, dul:Action6, dul:Action7, dul:Action8 .

dul:Action1 a dul:Action ;
    rdfs:label "Identify misplaced items on the countertop"@en ;
    soma:isPerformedBy psr:Robot ;
    psr:actsOn psr:Countertop ;
    psr:isAffordedBy soma:Perceiving .

dul:Action2 a dul:Action ;
    rdfs:label "Identify misplaced items on the dining table"@en ;
    soma:isPerformedBy psr:Robot ;
    psr:actsOn psr:DiningTable ;
    psr:isAffordedBy soma:Perceiving .

dul:Action3 a dul:Action ;
    rdfs:label "Pick up the yellow cup from the countertop"@en ;
    soma:isPerformedBy psr:Robot ;
    psr:actsOn psr:YellowCup ;
    psr:isAffordedBy soma:PickingUp .

dul:Action4 a dul:Action ;
    rdfs:label "Put the yellow cup in its storage location"@en ;
    soma:isPerformedBy psr:Robot ;
    psr:actsOn psr:YellowCup ;
    psr:isAffordedBy soma:PuttingDown .

dul:Action5 a dul:Action ;
    rdfs:label "Pick up the red jar from the countertop"@en ;
    soma:isPerformedBy psr:Robot ;
    psr:actsOn psr:RedJar ;
    psr:isAffordedBy soma:PickingUp .

dul:Action6 a dul:Action ;
    rdfs:label "Put the red jar in its storage location"@en ;
    soma:isPerformedBy psr:Robot ;
    psr:actsOn psr:RedJar ;
    psr:isAffordedBy soma:PuttingDown .

dul:Action7 a dul:Action ;
    rdfs:label "Pick up the large bottle of water from the dining table"@en ;
    soma:isPerformedBy psr:Robot ;
    psr:actsOn psr:LargeBottleOfWater ;
    psr:isAffordedBy soma:PickingUp .

dul:Action8 a dul:Action ;
    rdfs:label "Put the large bottle of water in its storage location"@en ;
    soma:isPerformedBy psr:Robot ;
    psr:actsOn psr:LargeBottleOfWater ;
    psr:isAffordedBy soma:PuttingDown .

dul:Action1 psr:precedes dul:Action3 .
dul:Action3 psr:precedes dul:Action4 .
dul:Action4 psr:precedes dul:Action5 .
dul:Action5 psr:precedes dul:Action6 .
dul:Action2 psr:precedes dul:Action7 .
dul:Action7 psr:precedes dul:Action8 .

psr:Countertop a psr:Component .
psr:DiningTable a psr:Component .
psr:YellowCup a psr:Object .
psr:RedJar a psr:Object .
psr:LargeBottleOfWater a psr:Object .
psr:Robot a dul:Agent .