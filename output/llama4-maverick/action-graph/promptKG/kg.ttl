@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/psr-task#> .

psr:RestoreKitchenToOrganizedState a psr:Instruction ;
    psr:hasNaturalLanguage "Restore the kitchen to an organized state by identifying all misplaced items and returning them to their standard storage locations based on their type and function. Prioritize actions according to logical task order, and perform each step atomically." ;
    psr:hasWorkflow psr:Workflow_RestoreKitchen .

psr:Workflow_RestoreKitchen a psr:Workflow ;
    psr:hasAction dul:Action_IdentifyMisplacedItems ;
    psr:hasAction dul:Action_PickUpMisplacedItems ;
    psr:hasAction dul:Action_ReturnItemsToStorage .

dul:Action_IdentifyMisplacedItems a dul:Action ;
    soma:isPerformedBy dul:Agent_Robot ;
    psr:actsOn psr:Component_MisplacedItems ;
    psr:precedes dul:Action_PickUpMisplacedItems .

dul:Action_PickUpMisplacedItems a dul:Action ;
    soma:isPerformedBy dul:Agent_Robot ;
    psr:actsOn psr:Component_MisplacedItems ;
    psr:isAffordedBy soma:PickingUp ;
    psr:precedes dul:Action_ReturnItemsToStorage ;
    dul:follows dul:Action_IdentifyMisplacedItems .

dul:Action_ReturnItemsToStorage a dul:Action ;
    soma:isPerformedBy dul:Agent_Robot ;
    psr:actsOn psr:Component_MisplacedItems ;
    psr:isAffordedBy soma:PuttingDown ;
    dul:follows dul:Action_PickUpMisplacedItems .

dul:Agent_Robot a dul:Agent .

psr:Component_MisplacedItems a psr:Component ;
    rdfs:comment "Misplaced items in the kitchen" .

soma:PickingUp a psr:Affordance .
soma:PuttingDown a psr:Affordance .

psr:Component_Table a psr:Component ;
    rdfs:label "Table" .

psr:Component_Cabinets a psr:Component ;
    rdfs:label "Cabinets" .

psr:Component_Countertop a psr:Component ;
    rdfs:label "Countertop" .

psr:Component_Floor a psr:Component ;
    rdfs:label "Floor" .

psr:Object_BoxOfCrackers a psr:Object ;
    rdfs:label "Box of crackers" .

psr:Object_PlasticBottleOfWater a psr:Object ;
    rdfs:label "Plastic bottle of water" .

psr:Object_DisposablePlasticCups a psr:Object ;
    rdfs:label "Disposable plastic cups" .

psr:Object_JarsWithLabel a psr:Object ;
    rdfs:label "Jars with label" .

psr:Object_BoxesOfJello a psr:Object ;
    rdfs:label "Boxes of jello" .

psr:Object_RiceBubbles a psr:Object ;
    rdfs:label "Rice bubbles" .

psr:Object_Crackers a psr:Object ;
    rdfs:label "Crackers" .

psr:Object_Cup a psr:Object ;
    rdfs:label "Cup" .