@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix psr: <https://w3id.org/psr-task#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

psr:Instruction_1 a psr:Instruction ;
    psr:hasNaturalLanguage "Restore the kitchen to an organized state by identifying all misplaced items and returning them to their standard storage locations based on their type and function."@en ;
    psr:hasWorkflow psr:Workflow_1 .

psr:Workflow_1 a psr:Workflow ;
    psr:hasAction soma:PickingUp_1, soma:PuttingDown_1, soma:PickingUp_2, soma:PuttingDown_2, soma:PickingUp_3, soma:PuttingDown_3 .

soma:PickingUp_1 a soma:PickingUp ;
    psr:actsOn psr:Object_1 ;
    psr:isAffordedBy psr:Affordance_1 .

psr:Object_1 a psr:Object ;
    rdfs:label "box of crackers"@en .

psr:Affordance_1 a psr:Affordance ;
    rdfs:label "Grasping"@en .

soma:PuttingDown_1 a soma:PuttingDown ;
    psr:actsOn psr:Object_1 ;
    psr:isAffordedBy psr:Affordance_2 .

psr:Affordance_2 a psr:Affordance ;
    rdfs:label "Releasing"@en .

soma:PickingUp_2 a soma:PickingUp ;
    psr:actsOn psr:Object_2 ;
    psr:isAffordedBy psr:Affordance_3 ;
    psr:follows soma:PuttingDown_1 .

psr:Object_2 a psr:Object ;
    rdfs:label "plastic bottle of water"@en .

psr:Affordance_3 a psr:Affordance ;
    rdfs:label "Grasping"@en .

soma:PuttingDown_2 a soma:PuttingDown ;
    psr:actsOn psr:Object_2 ;
    psr:isAffordedBy psr:Affordance_4 ;
    psr:follows soma:PickingUp_2 .

psr:Affordance_4 a psr:Affordance ;
    rdfs:label "Releasing"@en .

soma:PickingUp_3 a soma:PickingUp ;
    psr:actsOn psr:Object_3 ;
    psr:isAffordedBy psr:Affordance_5 ;
    psr:follows soma:PuttingDown_2 .

psr:Object_3 a psr:Object ;
    rdfs:label "disposable plastic cups"@en .

psr:Affordance_5 a psr:Affordance ;
    rdfs:label "Grasping"@en .

soma:PuttingDown_3 a soma:PuttingDown ;
    psr:actsOn psr:Object_3 ;
    psr:isAffordedBy psr:Affordance_6 ;
    psr:follows soma:PickingUp_3 .

psr:Affordance_6 a psr:Affordance ;
    rdfs:label "Releasing"@en .

soma:PickingUp_1 psr:isPerformedBy dul:Agent_1 .
dul:Agent_1 a dul:Agent .

soma:PuttingDown_1 psr:isPerformedBy dul:Agent_1 .
soma:PickingUp_2 psr:isPerformedBy dul:Agent_1 .
soma:PuttingDown_2 psr:isPerformedBy dul:Agent_1 .
soma:PickingUp_3 psr:isPerformedBy dul:Agent_1 .
soma:PuttingDown_3 psr:isPerformedBy dul:Agent_1 .