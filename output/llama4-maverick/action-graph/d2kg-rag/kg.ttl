@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix psr: <https://w3id.org/psr-task#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

psr:Task1 a psr:Instruction ;
    psr:hasNaturalLanguage "Restore the kitchen to an organized state"@en ;
    psr:hasWorkflow psr:Workflow1 .

psr:Workflow1 a psr:Workflow ;
    psr:hasAction soma:PickingUp1, soma:PuttingDown1, soma:PickingUp2, soma:PuttingDown2, soma:PickingUp3, soma:PuttingDown3, soma:PickingUp4, soma:PuttingDown4 .

soma:PickingUp1 a soma:PickingUp ;
    soma:isPerformedBy dul:Agent1 ;
    psr:actsOn psr:BoxOfJello ;
    psr:follows dul:Action0 .

soma:PuttingDown1 a soma:PuttingDown ;
    soma:isPerformedBy dul:Agent1 ;
    psr:actsOn psr:BoxOfJello ;
    psr:follows soma:PickingUp1 .

soma:PickingUp2 a soma:PickingUp ;
    soma:isPerformedBy dul:Agent1 ;
    psr:actsOn psr:BoxOfCrackers1 ;
    psr:follows soma:PuttingDown1 .

soma:PuttingDown2 a soma:PuttingDown ;
    soma:isPerformedBy dul:Agent1 ;
    psr:actsOn psr:BoxOfCrackers1 ;
    psr:follows soma:PickingUp2 .

soma:PickingUp3 a soma:PickingUp ;
    soma:isPerformedBy dul:Agent1 ;
    psr:actsOn psr:JarOfJam ;
    psr:follows soma:PuttingDown2 .

soma:PuttingDown3 a soma:PuttingDown ;
    soma:isPerformedBy dul:Agent1 ;
    psr:actsOn psr:JarOfJam ;
    psr:follows soma:PickingUp3 .

soma:PickingUp4 a soma:PickingUp ;
    soma:isPerformedBy dul:Agent1 ;
    psr:actsOn psr:BottleOfWater1 ;
    psr:follows soma:PuttingDown3 .

soma:PuttingDown4 a soma:PuttingDown ;
    soma:isPerformedBy dul:Agent1 ;
    psr:actsOn psr:BottleOfWater1 ;
    psr:follows soma:PickingUp4 .

dul:Agent1 a dul:Agent .

psr:BoxOfJello a psr:Object ;
    psr:hasAffordance soma:PickingUp1, soma:PuttingDown1 .

psr:BoxOfCrackers1 a psr:Object ;
    psr:hasAffordance soma:PickingUp2, soma:PuttingDown2 .

psr:JarOfJam a psr:Object ;
    psr:hasAffordance soma:PickingUp3, soma:PuttingDown3 .

psr:BottleOfWater1 a psr:Object ;
    psr:hasAffordance soma:PickingUp4, soma:PuttingDown4 .

dul:Action0 a dul:Action .

psr:Component1 a psr:Component ;
    rdfs:label "black kitchen cabinets"@en .

psr:Component2 a psr:Component ;
    rdfs:label "white refrigerator"@en .

psr:Component3 a psr:Component ;
    rdfs:label "marble countertop"@en .

psr:Component4 a psr:Component ;
    rdfs:label "kitchen sink"@en .

psr:Component5 a psr:Component ;
    rdfs:label "electric stove"@en .

psr:Component6 a psr:Component ;
    rdfs:label "wooden range hood"@en .

psr:Component7 a psr:Component ;
    rdfs:label "wooden dining table"@en .

psr:Component8 a psr:Component ;
    rdfs:label "white chairs"@en .

psr:Object1 a psr:Object ;
    rdfs:label "box of jello"@en .

psr:Object2 a psr:Object ;
    rdfs:label "box of crackers"@en .

psr:Object3 a psr:Object ;
    rdfs:label "bottle of water"@en .

psr:Object4 a psr:Object ;
    rdfs:label "stack of plates"@en .

psr:Object5 a psr:Object ;
    rdfs:label "jar of jam"@en .

psr:Object6 a psr:Object ;
    rdfs:label "jug"@en .