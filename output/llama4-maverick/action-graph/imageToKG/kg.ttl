@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/psr-task#> .

psr:Instruction1 a psr:Instruction ;
    psr:hasNaturalLanguage "Restore the kitchen to an organized state"@en ;
    psr:hasWorkflow psr:Workflow1 .

psr:Workflow1 a psr:Workflow ;
    psr:hasAction psr:Action1, psr:Action2, psr:Action3, psr:Action4, psr:Action5, psr:Action6, psr:Action7, psr:Action8 .

psr:Action1 a dul:Action ;
    rdfs:label "Pick up the box of Chicky"@en ;
    soma:isPerformedBy psr:Robot ;
    psr:actsOn psr:BoxOfChicky ;
    psr:isAffordedBy soma:PickingUp .

psr:Action2 a dul:Action ;
    rdfs:label "Pick up the jar of Orange Juice"@en ;
    soma:isPerformedBy psr:Robot ;
    psr:actsOn psr:JarOfOrangeJuice ;
    psr:isAffordedBy soma:PickingUp .

psr:Action3 a dul:Action ;
    rdfs:label "Pick up the jar of Strawberry Jam"@en ;
    soma:isPerformedBy psr:Robot ;
    psr:actsOn psr:JarOfStrawberryJam ;
    psr:isAffordedBy soma:PickingUp .

psr:Action4 a dul:Action ;
    rdfs:label "Open the cabinet"@en ;
    soma:isPerformedBy psr:Robot ;
    psr:actsOn psr:Cabinet ;
    psr:isAffordedBy soma:Opening .

psr:Action5 a dul:Action ;
    rdfs:label "Place the box of Chicky inside the cabinet"@en ;
    soma:isPerformedBy psr:Robot ;
    psr:actsOn psr:BoxOfChicky ;
    psr:isAffordedBy soma:PuttingDown .

psr:Action6 a dul:Action ;
    rdfs:label "Place the jar of Orange Juice inside the cabinet"@en ;
    soma:isPerformedBy psr:Robot ;
    psr:actsOn psr:JarOfOrangeJuice ;
    psr:isAffordedBy soma:PuttingDown .

psr:Action7 a dul:Action ;
    rdfs:label "Place the jar of Strawberry Jam inside the cabinet"@en ;
    soma:isPerformedBy psr:Robot ;
    psr:actsOn psr:JarOfStrawberryJam ;
    psr:isAffordedBy soma:PuttingDown .

psr:Action8 a dul:Action ;
    rdfs:label "Close the cabinet"@en ;
    soma:isPerformedBy psr:Robot ;
    psr:actsOn psr:Cabinet ;
    psr:isAffordedBy soma:Closing .

psr:BoxOfChicky a psr:Object .

psr:JarOfOrangeJuice a psr:Object .

psr:JarOfStrawberryJam a psr:Object .

psr:Cabinet a psr:Furniture .

psr:Robot a dul:Agent .

psr:Action1 psr:precedes psr:Action2 .
psr:Action2 psr:precedes psr:Action3 .
psr:Action3 psr:precedes psr:Action4 .
psr:Action4 psr:precedes psr:Action5 .
psr:Action5 psr:precedes psr:Action6 .
psr:Action6 psr:precedes psr:Action7 .
psr:Action7 psr:precedes psr:Action8 .