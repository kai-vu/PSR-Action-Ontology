@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/psr-task#> .

psr:Instruction_1 a psr:Instruction ;
    psr:hasWorkflow psr:Workflow_1 ;
    psr:hasNaturalLanguage "Restore the kitchen to an organized state."@en .

psr:Workflow_1 a psr:Workflow ;
    psr:hasAction psr:Action_1, psr:Action_2, psr:Action_3, psr:Action_4, psr:Action_5, psr:Action_6, psr:Action_7, psr:Action_8 .

psr:Action_1 a dul:Action ;
    soma:isPerformedBy psr:Robot_1 ;
    psr:actsOn psr:Table_1 ;
    psr:isAffordedBy soma:PickingUp ;
    rdfs:label "Pick up the bottle on the table"@en ;
    psr:precedes psr:Action_2 .

psr:Action_2 a dul:Action ;
    soma:isPerformedBy psr:Robot_1 ;
    psr:actsOn psr:Fridge_1 ;
    psr:isAffordedBy soma:Opening ;
    rdfs:label "Open the fridge"@en ;
    psr:precedes psr:Action_3 .

psr:Action_3 a dul:Action ;
    soma:isPerformedBy psr:Robot_1 ;
    psr:actsOn psr:Bottle_1 ;
    psr:isAffordedBy soma:PuttingDown ;
    rdfs:label "Put the bottle in the fridge"@en ;
    psr:precedes psr:Action_4 .

psr:Action_4 a dul:Action ;
    soma:isPerformedBy psr:Robot_1 ;
    psr:actsOn psr:Fridge_1 ;
    psr:isAffordedBy soma:Closing ;
    rdfs:label "Close the fridge"@en ;
    psr:precedes psr:Action_5 .

psr:Action_5 a dul:Action ;
    soma:isPerformedBy psr:Robot_1 ;
    psr:actsOn psr:Countertop_1 ;
    psr:actsOn psr:Jar_1 ;
    psr:isAffordedBy soma:PickingUp ;
    rdfs:label "Pick up the jam jar from the countertop"@en ;
    psr:precedes psr:Action_6 .

psr:Action_6 a dul:Action ;
    soma:isPerformedBy psr:Robot_1 ;
    psr:actsOn psr:Cabinet_1 ;
    psr:isAffordedBy soma:Opening ;
    rdfs:label "Open the cabinet"@en ;
    psr:precedes psr:Action_7 .

psr:Action_7 a dul:Action ;
    soma:isPerformedBy psr:Robot_1 ;
    psr:actsOn psr:Jar_1 ;
    psr:isAffordedBy soma:PuttingDown ;
    rdfs:label "Put the jam jar in the cabinet"@en ;
    psr:precedes psr:Action_8 .

psr:Action_8 a dul:Action ;
    soma:isPerformedBy psr:Robot_1 ;
    psr:actsOn psr:Cabinet_1 ;
    psr:isAffordedBy soma:Closing ;
    rdfs:label "Close the cabinet"@en .

psr:Robot_1 a dul:Agent .
psr:Table_1 a psr:Furniture .
psr:Fridge_1 a psr:Appliance .
psr:Bottle_1 a psr:Object .
psr:Countertop_1 a psr:Furniture .
psr:Jar_1 a psr:Object .
psr:Cabinet_1 a psr:Furniture .