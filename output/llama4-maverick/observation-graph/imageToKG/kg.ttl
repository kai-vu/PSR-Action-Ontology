@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/psr-task#> .

psr:KitchenEnvironment a psr:Environment ;
    dul:hasComponent psr:KitchenCabinet, psr:DiningTable, psr:Fridge, psr:Sink .

psr:KitchenCabinet a psr:Appliance ;
    dul:hasLocation psr:KitchenCounter ;
    psr:hasAffordance soma:Opening, soma:Closing .

psr:DiningTable a psr:Furniture ;
    dul:hasLocation psr:DiningArea ;
    psr:hasAffordance soma:Holding .

psr:Fridge a psr:Appliance ;
    dul:hasLocation psr:KitchenArea ;
    psr:hasAffordance soma:Opening, soma:Closing .

psr:Sink a psr:Appliance ;
    dul:hasLocation psr:KitchenCounter ;
    psr:hasAffordance soma:Holding .

psr:KitchenCounter a psr:Furniture ;
    dul:hasLocation psr:KitchenArea ;
    psr:hasComponent psr:Jar, psr:Box, psr:Plate, psr:Mug, psr:Pot .

psr:Jar a psr:Object ;
    dul:hasLocation psr:KitchenCounter ;
    psr:onTopOf psr:KitchenCounter .

psr:Box a psr:Object ;
    dul:hasLocation psr:KitchenCounter ;
    psr:onTopOf psr:KitchenCounter .

psr:Plate a psr:Object ;
    dul:hasLocation psr:KitchenCounter, psr:DiningTable ;
    psr:onTopOf psr:KitchenCounter, psr:DiningTable .

psr:Mug a psr:Object ;
    dul:hasLocation psr:DiningTable, psr:KitchenCounter ;
    psr:onTopOf psr:DiningTable, psr:KitchenCounter .

psr:Pot a psr:Object ;
    dul:hasLocation psr:KitchenCounter ;
    psr:onTopOf psr:KitchenCounter .

psr:DiningArea a soma:Location ;
    geo:sfContains psr:DiningTable .

psr:KitchenArea a soma:Location ;
    geo:sfContains psr:Fridge, psr:KitchenCabinet, psr:KitchenCounter .

psr:KitchenCounter a soma:Location ;
    geo:sfContains psr:Jar, psr:Box, psr:Plate, psr:Mug, psr:Pot .

psr:DiningTable a soma:Location ;
    geo:sfContains psr:Plate, psr:Mug .