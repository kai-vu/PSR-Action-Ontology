@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/psr-task#> .

psr:KitchenEnvironment a psr:Environment ;
    dul:hasComponent psr:Table1, psr:Chair1, psr:Chair2, psr:Chair3, psr:Refrigerator, psr:Cabinet1, psr:Cabinet2, psr:Sink .

psr:Table1 a psr:Furniture ;
    dul:hasLocation psr:Room1 .

psr:Chair1 a psr:Furniture ;
    dul:hasLocation psr:Room1 .

psr:Chair2 a psr:Furniture ;
    dul:hasLocation psr:Room1 .

psr:Chair3 a psr:Furniture ;
    dul:hasLocation psr:Room1 .

psr:Refrigerator a psr:Appliance ;
    dul:hasLocation psr:Room1 ;
    psr:hasAffordance soma:Opening, soma:Closing .

psr:Cabinet1 a psr:Furniture ;
    dul:hasLocation psr:Room1 ;
    psr:hasAffordance soma:Opening, soma:Closing .

psr:Cabinet2 a psr:Furniture ;
    dul:hasLocation psr:Room1 ;
    psr:hasAffordance soma:Opening, soma:Closing .

psr:Sink a psr:Furniture ;
    dul:hasLocation psr:Room1 .

psr:Room1 a soma:Location .

psr:Jar1 a psr:Object ;
    dul:hasLocation psr:Cabinet1 .

psr:Jar2 a psr:Object ;
    dul:hasLocation psr:Table1 .

psr:Box1 a psr:Object ;
    dul:hasLocation psr:Cabinet1 .

psr:Box2 a psr:Object ;
    dul:hasLocation psr:Table1 .

psr:Bottle1 a psr:Object ;
    dul:hasLocation psr:Table1 .

psr:Glass1 a psr:Object ;
    dul:hasLocation psr:Table1 .

psr:Plate1 a psr:Object ;
    dul:hasLocation psr:Cabinet1 .

psr:Pans1 a psr:Object ;
    dul:hasLocation psr:Cabinet2 .

psr:Book1 a psr:Object ;
    dul:hasLocation psr:Table1 .

geo:sfContains(psr:Room1, psr:Table1) .
geo:sfContains(psr:Room1, psr:Chair1) .
geo:sfContains(psr:Room1, psr:Chair2) .
geo:sfContains(psr:Room1, psr:Chair3) .
geo:sfContains(psr:Room1, psr:Refrigerator) .
geo:sfContains(psr:Room1, psr:Cabinet1) .
geo:sfContains(psr:Room1, psr:Cabinet2) .
geo:sfContains(psr:Cabinet1, psr:Jar1) .
geo:sfContains(psr:Cabinet1, psr:Box1) .
geo:sfContains(psr:Cabinet1, psr:Plate1) .
geo:sfContains(psr:Table1, psr:Jar2) .
geo:sfContains(psr:Table1, psr:Box2) .
geo:sfContains(psr:Table1, psr:Bottle1) .
geo:sfContains(psr:Table1, psr:Glass1) .
geo:sfContains(psr:Table1, psr:Book1) .
geo:sfContains(psr:Cabinet2, psr:Pans1) .