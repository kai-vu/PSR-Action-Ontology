@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/psr-task#> .

psr:Kitchen a psr:Environment ;
    dul:hasComponent psr:DarkWallCabinets, psr:LightMarbleCountertop, psr:BlueFramedMirror, psr:WhiteRefrigerator, psr:SilverSink, psr:ElectricStove, psr:RedBox1, psr:BlueBox1, psr:YellowCup, psr:RedJar, psr:StackOfWhitePlates, psr:Pot, psr:RangeHood, psr:BrownWoodenDiningTable, psr:WhiteChairs, psr:LargeBottleOfWater, psr:BoxOnTable, psr:Mug, psr:WhiteDoor .

psr:DarkWallCabinets a psr:Furniture ;
    dul:hasLocation psr:CurrentLocation_DarkWallCabinets .

psr:CurrentLocation_DarkWallCabinets a psr:CurrentLocation ;
    psr:onTopOf psr:LightMarbleCountertop .

psr:LightMarbleCountertop a psr:Furniture ;
    dul:hasLocation psr:CurrentLocation_LightMarbleCountertop ;
    psr:hasAffordance soma:Holding .

psr:CurrentLocation_LightMarbleCountertop a psr:CurrentLocation .

psr:BlueFramedMirror a psr:Object ;
    dul:hasLocation psr:CurrentLocation_BlueFramedMirror .

psr:CurrentLocation_BlueFramedMirror a psr:CurrentLocation ;
    geo:sfWithin psr:DarkWallCabinets .

psr:WhiteRefrigerator a psr:Appliance ;
    dul:hasLocation psr:CurrentLocation_WhiteRefrigerator .

psr:CurrentLocation_WhiteRefrigerator a psr:CurrentLocation ;
    geo:sfWithin psr:DarkWallCabinets .

psr:SilverSink a psr:Object ;
    dul:hasLocation psr:CurrentLocation_SilverSink ;
    psr:hasAffordance soma:Holding .

psr:CurrentLocation_SilverSink a psr:CurrentLocation ;
    geo:sfWithin psr:LightMarbleCountertop .

psr:ElectricStove a psr:Appliance ;
    dul:hasLocation psr:CurrentLocation_ElectricStove ;
    psr:hasAffordance soma:Holding .

psr:CurrentLocation_ElectricStove a psr:CurrentLocation ;
    geo:sfWithin psr:LightMarbleCountertop .

psr:RedBox1 a psr:Object ;
    dul:hasLocation psr:CurrentLocation_RedBox1 .

psr:CurrentLocation_RedBox1 a psr:CurrentLocation ;
    geo:sfWithin psr:LightMarbleCountertop .

psr:BlueBox1 a psr:Object ;
    dul:hasLocation psr:CurrentLocation_BlueBox1 .

psr:CurrentLocation_BlueBox1 a psr:CurrentLocation ;
    geo:sfWithin psr:LightMarbleCountertop .

psr:YellowCup a psr:Object ;
    dul:hasLocation psr:CurrentLocation_YellowCup .

psr:CurrentLocation_YellowCup a psr:CurrentLocation ;
    geo:sfWithin psr:LightMarbleCountertop .

psr:RedJar a psr:Object ;
    dul:hasLocation psr:CurrentLocation_RedJar .

psr:CurrentLocation_RedJar a psr:CurrentLocation ;
    geo:sfWithin psr:LightMarbleCountertop .

psr:StackOfWhitePlates a psr:Object ;
    dul:hasLocation psr:CurrentLocation_StackOfWhitePlates .

psr:CurrentLocation_StackOfWhitePlates a psr:CurrentLocation ;
    geo:sfWithin psr:LightMarbleCountertop .

psr:Pot a psr:Object ;
    dul:hasLocation psr:CurrentLocation_Pot .

psr:CurrentLocation_Pot a psr:CurrentLocation ;
    geo:sfWithin psr:ElectricStove .

psr:RangeHood a psr:Appliance ;
    dul:hasLocation psr:CurrentLocation_RangeHood .

psr:CurrentLocation_RangeHood a psr:CurrentLocation ;
    psr:onTopOf psr:ElectricStove .

psr:BrownWoodenDiningTable a psr:Furniture ;
    dul:hasLocation psr:CurrentLocation_BrownWoodenDiningTable .

psr:CurrentLocation_BrownWoodenDiningTable a psr:CurrentLocation .

psr:WhiteChairs a psr:Furniture ;
    dul:hasLocation psr:CurrentLocation_WhiteChairs .

psr:CurrentLocation_WhiteChairs a psr:CurrentLocation ;
    geo:sfWithin psr:BrownWoodenDiningTable .

psr:LargeBottleOfWater a psr:Object ;
    dul:hasLocation psr:CurrentLocation_LargeBottleOfWater .

psr:CurrentLocation_LargeBottleOfWater a psr:CurrentLocation ;
    geo:sfWithin psr:BrownWoodenDiningTable .

psr:BoxOnTable a psr:Object ;
    dul:hasLocation psr:CurrentLocation_BoxOnTable .

psr:CurrentLocation_BoxOnTable a psr:CurrentLocation ;
    geo:sfWithin psr:BrownWoodenDiningTable .

psr:Mug a psr:Object ;
    dul:hasLocation psr:CurrentLocation_Mug .

psr:CurrentLocation_Mug a psr:CurrentLocation ;
    geo:sfWithin psr:BrownWoodenDiningTable .

psr:WhiteDoor a psr:Object ;
    dul:hasLocation psr:CurrentLocation_WhiteDoor .

psr:CurrentLocation_WhiteDoor a psr:CurrentLocation .