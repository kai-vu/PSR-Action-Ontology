@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/psr-task#> .

psr:KitchenEnvironment a psr:Environment ;
    dul:hasComponent psr:KitchenCabinets, psr:Refrigerator, psr:Countertop, psr:DiningTable, psr:WhiteChairs, psr:Sink, psr:WoodenFramedMirror, psr:RangeHood, psr:Stove, psr:BoxOfCrackers, psr:PlasticBottleOfWater, psr:DisposablePlasticCups, psr:JarsWithLabel, psr:BoxesOfJello, psr:RiceBubbles, psr:Crackers, psr:Cup .

psr:KitchenCabinets a psr:Furniture ;
    psr:hasAffordance soma:Opening, soma:Closing .

psr:Refrigerator a psr:Appliance ;
    dul:hasLocation psr:RefrigeratorLocation .

psr:RefrigeratorLocation a psr:CurrentLocation ;
    geo:sfBehind psr:KitchenCabinets .

psr:Countertop a psr:Furniture ;
    psr:hasAffordance soma:Holding, soma:PuttingDown .

psr:DiningTable a psr:Furniture ;
    psr:hasAffordance soma:Holding, soma:PuttingDown .

psr:WhiteChairs a psr:Furniture ;
    dul:hasLocation psr:DiningArea .

psr:Sink a psr:Object ;
    dul:hasLocation psr:KitchenCounter .

psr:WoodenFramedMirror a psr:Object ;
    dul:hasLocation psr:WallAboveSink .

psr:RangeHood a psr:Object ;
    dul:hasLocation psr:AboveStove .

psr:Stove a psr:Appliance .

psr:BoxOfCrackers a psr:Object ;
    dul:hasLocation psr:OnTable .

psr:PlasticBottleOfWater a psr:Object ;
    dul:hasLocation psr:OnTable .

psr:DisposablePlasticCups a psr:Object ;
    dul:hasLocation psr:OnTable .

psr:JarsWithLabel a psr:Object ;
    dul:hasLocation psr:OnCountertop .

psr:BoxesOfJello a psr:Object ;
    dul:hasLocation psr:OnCountertop .

psr:RiceBubbles a psr:Object ;
    dul:hasLocation psr:OnCountertop .

psr:Crackers a psr:Object ;
    dul:hasLocation psr:OnCountertop .

psr:Cup a psr:Object ;
    dul:hasLocation psr:OnCountertop .

psr:OnTable a psr:CurrentLocation ;
    psr:onTopOf psr:DiningTable .

psr:OnCountertop a psr:CurrentLocation ;
    psr:onTopOf psr:Countertop .

psr:WallAboveSink a psr:CurrentLocation ;
    geo:sfWithin psr:KitchenArea .

psr:AboveStove a psr:CurrentLocation ;
    geo:sfWithin psr:KitchenArea .

psr:DiningArea a psr:Location ;
    geo:sfWithin psr:KitchenEnvironment .

psr:KitchenArea a psr:Location ;
    geo:sfWithin psr:KitchenEnvironment .

psr:KitchenCounter a psr:Location ;
    geo:sfWithin psr:KitchenArea .

psr:Floor a psr:Location ;
    geo:sfWithin psr:KitchenEnvironment .

psr:WhiteWalls a psr:Component ;
    dul:hasLocation psr:KitchenEnvironment .

psr:DarkWoodFlooring a psr:Component ;
    dul:hasLocation psr:Floor .

psr:RoundLightFixture a psr:Component ;
    dul:hasLocation psr:Ceiling .

psr:Ceiling a psr:Location ;
    geo:sfWithin psr:KitchenEnvironment .