@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/psr-task#> .

psr:Kitchen a psr:Environment ;
    dul:hasComponent psr:BlackKitchenCabinets, psr:WhiteRefrigerator, psr:MarbleCountertop, psr:KitchenSink, psr:ElectricStove, psr:WoodenRangeHood, psr:WhiteWalls, psr:BrownParquetFlooring, psr:RoundCeilingLight, psr:WoodenDiningTable, psr:WhiteChairs, psr:BoxOfJello, psr:BoxOfCrackers, psr:BottleOfWater, psr:StackOfPlates, psr:JarOfJam, psr:Jug .

psr:BlackKitchenCabinets a psr:Appliance ;
    psr:hasAffordance soma:Opening, soma:Closing ;
    dul:hasLocation psr:KitchenCabinetsLocation .

psr:WhiteRefrigerator a psr:Appliance ;
    psr:hasAffordance soma:Opening, soma:Closing ;
    dul:hasLocation psr:RefrigeratorLocation .

psr:MarbleCountertop a psr:Furniture ;
    psr:hasAffordance soma:Holding ;
    dul:hasLocation psr:CountertopLocation ;
    geo:sfContains psr:BoxOfJello, psr:BoxOfCrackers, psr:JarOfJam, psr:BottleOfWater, psr:StackOfPlates .

psr:KitchenSink a psr:Object ;
    dul:hasLocation psr:SinkLocation .

psr:ElectricStove a psr:Appliance ;
    dul:hasLocation psr:StoveLocation ;
    geo:sfWithin psr:Kitchen .

psr:WoodenRangeHood a psr:Appliance ;
    dul:hasLocation psr:RangeHoodLocation .

psr:WhiteWalls a psr:Furniture ;
    dul:hasLocation psr:WallsLocation .

psr:BrownParquetFlooring a psr:Furniture ;
    dul:hasLocation psr:FlooringLocation .

psr:RoundCeilingLight a psr:Object ;
    dul:hasLocation psr:CeilingLightLocation .

psr:WoodenDiningTable a psr:Furniture ;
    psr:hasAffordance soma:Holding ;
    dul:hasLocation psr:DiningTableLocation ;
    geo:sfContains psr:BottleOfWater, psr:BoxOfCrackers, psr:Jug .

psr:WhiteChairs a psr:Furniture ;
    dul:hasLocation psr:ChairsLocation .

psr:BoxOfJello a psr:Object ;
    dul:hasLocation psr:CountertopLocation .

psr:BoxOfCrackers a psr:Object ;
    dul:hasLocation psr:CountertopLocation, psr:DiningTableLocation .

psr:BottleOfWater a psr:Object ;
    dul:hasLocation psr:CountertopLocation, psr:DiningTableLocation .

psr:StackOfPlates a psr:Object ;
    dul:hasLocation psr:CountertopLocation .

psr:JarOfJam a psr:Object ;
    dul:hasLocation psr:CountertopLocation .

psr:Jug a psr:Object ;
    dul:hasLocation psr:DiningTableLocation .

psr:RefrigeratorLocation geo:sfWithin psr:Kitchen ;
    geo:sfOverlaps psr:DiningTableLocation .

psr:StoveLocation psr:onTopOf psr:CountertopLocation .
psr:SinkLocation geo:sfWithin psr:Kitchen ;
    geo:sfOverlaps psr:StoveLocation .

psr:RangeHoodLocation psr:onTopOf psr:StoveLocation .

psr:DiningTableLocation geo:sfWithin psr:Kitchen .