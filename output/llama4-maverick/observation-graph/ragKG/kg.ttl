@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix psr: <https://w3id.org/psrTask-ontology#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .

# Defining the kitchen environment
psr:KitchenEnvironment a psr:Environment ;
    rdfs:label "Kitchen Environment"@en .

# Defining components in the kitchen
psr:KitchenCounter a psr:Component ;
    rdfs:label "Kitchen Counter"@en ;
    psr:hasAffordance soma:Holding .

psr:WhiteRefrigerator a psr:Appliance ;
    rdfs:label "White Refrigerator"@en ;
    psr:hasAffordance soma:Opening, soma:Closing .

psr:BoxOfJello a psr:Object ;
    rdfs:label "Box of Jello"@en ;
    dul:hasLocation psr:KitchenCounter .

psr:JarOfJam a psr:Object ;
    rdfs:label "Jar of Jam"@en ;
    dul:hasLocation psr:KitchenCounter .

psr:BoxOfSalt a psr:Object ;
    rdfs:label "Box of Salt"@en ;
    dul:hasLocation psr:KitchenCounter .

psr:StackOfPlates a psr:Object ;
    rdfs:label "Stack of Plates"@en ;
    dul:hasLocation psr:KitchenCounter .

psr:DiningTable a psr:Furniture ;
    rdfs:label "Dining Table"@en ;
    psr:hasAffordance soma:Holding .

psr:BottleOfWater a psr:Object ;
    rdfs:label "Bottle of Water"@en ;
    dul:hasLocation psr:DiningTable .

psr:BoxOfCrackers a psr:Object ;
    rdfs:label "Box of Crackers"@en ;
    dul:hasLocation psr:DiningTable .

psr:Mug a psr:Object ;
    rdfs:label "Mug"@en ;
    dul:hasLocation psr:DiningTable .

psr:Sink a psr:Component ;
    rdfs:label "Sink"@en ;
    dul:hasLocation psr:KitchenCounter .

psr:Pot a psr:Object ;
    rdfs:label "Pot"@en ;
    dul:hasLocation psr:KitchenCounter .

psr:StoveTop a psr:Appliance ;
    rdfs:label "Stove Top"@en ;
    psr:hasAffordance soma:TurningOn, soma:TurningOff .

psr:RangeHood a psr:Appliance ;
    rdfs:label "Range Hood"@en ;
    dul:hasLocation psr:StoveTop .

psr:BoxOfChuckyCereal a psr:Object ;
    rdfs:label "Box of Chucky Cereal"@en ;
    dul:hasLocation psr:KitchenCounter .

psr:BoxOfRiceBubbles a psr:Object ;
    rdfs:label "Box of Rice Bubbles"@en ;
    dul:hasLocation psr:KitchenCounter .

psr:WineGlass a psr:Object ;
    rdfs:label "Wine Glass"@en ;
    dul:hasLocation psr:DiningTable .

# Defining locations and relations
psr:KitchenCounterLocation a soma:Location ;
    geo:sfWithin psr:KitchenEnvironment .

psr:DiningTableLocation a soma:Location ;
    geo:sfWithin psr:KitchenEnvironment .

psr:WhiteRefrigerator dul:hasLocation psr:KitchenCounterLocation .
psr:DiningTable dul:hasLocation psr:DiningTableLocation .

# Relations between components and their locations
psr:KitchenCounter dul:hasComponent psr:BoxOfJello, psr:JarOfJam, psr:BoxOfSalt, psr:StackOfPlates .
psr:DiningTable dul:hasComponent psr:BottleOfWater, psr:BoxOfCrackers, psr:Mug, psr:WineGlass .

# Spatial relations
psr:WhiteRefrigerator geo:sfWithin psr:KitchenEnvironment .
psr:DiningTable geo:sfWithin psr:KitchenEnvironment .
psr:Sink geo:sfWithin psr:KitchenCounter .
psr:Pot geo:sfWithin psr:KitchenCounter .
psr:StoveTop geo:sfWithin psr:KitchenCounter .
psr:RangeHood geo:sfWithin psr:StoveTop .