@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/psr-task#> .

psr:Kitchen a psr:Environment ;
    dul:hasComponent psr:Table, psr:Chairs, psr:Cabinet, psr:Fridge, psr:Sink, psr:Stove, psr:ExhaustFan, psr:Door .

psr:Table a psr:Furniture ;
    dul:hasLocation psr:DiningArea ;
    psr:hasAffordance soma:Holding .

psr:Chairs a psr:Furniture ;
    dul:hasLocation psr:DiningArea ;
    psr:hasAffordance soma:Sitting .

psr:Cabinet a psr:Furniture ;
    dul:hasLocation psr:KitchenArea ;
    psr:hasAffordance soma:Opening, soma:Closing .

psr:Fridge a psr:Appliance ;
    dul:hasLocation psr:KitchenArea ;
    psr:hasAffordance soma:Opening, soma:Closing .

psr:Sink a psr:Appliance ;
    dul:hasLocation psr:KitchenArea ;
    psr:hasAffordance soma:Containing .

psr:Stove a psr:Appliance ;
    dul:hasLocation psr:KitchenArea ;
    psr:hasAffordance soma:Heating .

psr:ExhaustFan a psr:Appliance ;
    dul:hasLocation psr:KitchenArea ;
    psr:hasAffordance soma:Ventilating .

psr:Door a psr:Furniture ;
    dul:hasLocation psr:DiningArea ;
    psr:hasAffordance soma:Opening, soma:Closing .

psr:DiningArea a soma:Location ;
    geo:sfContains psr:Table, psr:Chairs .

psr:KitchenArea a soma:Location ;
    geo:sfContains psr:Cabinet, psr:Fridge, psr:Sink, psr:Stove, psr:ExhaustFan .

psr:JelloBox a psr:Object ;
    dul:hasLocation psr:CounterTop ;
    psr:onTopOf psr:Cabinet .

psr:AlpenSalt a psr:Object ;
    dul:hasLocation psr:CounterTop ;
    psr:onTopOf psr:Cabinet .

psr:ChunkyBox a psr:Object ;
    dul:hasLocation psr:CounterTop ;
    psr:onTopOf psr:Cabinet .

psr:RiceBubblesBox a psr:Object ;
    dul:hasLocation psr:CounterTop ;
    psr:onTopOf psr:Cabinet .

psr:Plates a psr:Object ;
    dul:hasLocation psr:CounterTop ;
    psr:onTopOf psr:Cabinet .

psr:WaterBottle a psr:Object ;
    dul:hasLocation psr:Table ;
    psr:onTopOf psr:Table .

psr:FruitBox a psr:Object ;
    dul:hasLocation psr:Table ;
    psr:onTopOf psr:Table .

psr:CounterTop a soma:Location ;
    geo:sfContains psr:JelloBox, psr:AlpenSalt, psr:ChunkyBox, psr:RiceBubblesBox, psr:Plates .

psr:Cabinet a psr:Furniture ;
    dul:hasLocation psr:KitchenArea .

psr:Fridge a psr:Appliance ;
    dul:hasLocation psr:KitchenArea .

psr:Sink a psr:Appliance ;
    dul:hasLocation psr:CounterTop .

psr:Stove a psr:Appliance ;
    dul:hasLocation psr:CounterTop .

psr:ExhaustFan a psr:Appliance ;
    dul:hasLocation psr:KitchenArea .

psr:Door a psr:Furniture ;
    dul:hasLocation psr:DiningArea .

psr:WaterBottle a psr:Object ;
    dul:hasLocation psr:Table .

psr:FruitBox a psr:Object ;
    dul:hasLocation psr:Table .