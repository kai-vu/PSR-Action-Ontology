@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/psr-task#> .

psr:RestoreKitchenToOrganizedState a psr:Instruction ;
    psr:hasNaturalLanguage "Restore the kitchen to an organized state by identifying all misplaced items and returning them to their standard storage locations based on their type and function. Prioritize actions according to logical task order, and perform each step atomically."^^xsd:string ;
    psr:hasWorkflow psr:RestoreKitchenWorkflow .

psr:RestoreKitchenWorkflow a psr:Workflow ;
    psr:hasAction dul:IdentifyMisplacedItems ;
    psr:hasAction dul:ReturnItemsToStorageLocations ;
    dul:precedes dul:ReturnItemsToStorageLocations, dul:IdentifyMisplacedItems .

dul:IdentifyMisplacedItems a dul:Action ;
    soma:isPerformedBy psr:Robot ;
    psr:actsOn psr:Kitchen .

dul:ReturnItemsToStorageLocations a dul:Action ;
    soma:isPerformedBy psr:Robot ;
    psr:actsOn psr:MisplacedItems .

psr:Kitchen a psr:Component .
psr:MisplacedItems a psr:Component .

dul:IdentifyMisplacedItems psr:isAffordedBy soma:PickingUp .
dul:ReturnItemsToStorageLocations psr:isAffordedBy soma:PuttingDown .

soma:PickingUp a psr:Affordance .
soma:PuttingDown a psr:Affordance .

psr:Robot a dul:Agent .

psr:Plates a psr:Object ;
    rdf:type psr:Component .

