  psr:isAffordedBy soma:PickingUp .

psr:Pick_Up_Items a dul:Action ;
  psr:actsOn psr:Alpen_Salt_Box ;
  psr:isAffordedBy soma:PickingUp .

psr:Pick_Up_Items a dul:Action ;
  psr:actsOn psr:Peanut_Butter_Jar ;
  psr:isAffordedBy soma:PickingUp .

psr:Put_Items_Into_Cabinet a dul:Action ;
  psr:actsOn psr:M&M's_Box ;
  psr:isAffordedBy soma:PuttingDown .

psr:Put_Items_Into_Cabinet a dul:Action ;
