@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix psr: <https://w3id.org/psr-task#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix :       <http://example.org/data/> .

:Kitchen a psr:Environment .
:Refrigerator a psr:Appliance ;
    dul:hasLocation :Countertop .
:DiningTable a psr:Furniture ;
    dul:hasLocation :Kitchen .
:Countertop a psr:Component ;
    dul:hasLocation :Kitchen .
:Oven a psr:Appliance ;
    dul:hasLocation :Kitchen .
:Sinks a psr:Component ;
    dul:hasLocation :Kitchen .
:LightFixture a psr:Component ;
    dul:hasLocation :Kitchen .
:TrashCan a psr:Object ;
    dul:hasLocation :UnderWindow .
:UnderWindow a psr:Location ;
    dul:hasLocation :Kitchen .
:Window a psr:Component ;
    dul:hasLocation :Kitchen .
:GlassDoor a psr:Component ;
    dul:hasLocation :Kitchen .
:LightSwitch a psr:Component ;
    dul:hasLocation :Hallway .
:Hallway a psr:Environment ;
    dul:hasLocation :Kitchen .
:WhiteDoor a psr:Component ;
    dul:hasLocation :Hallway .
:Cups a psr:Object ;
    dul:hasLocation :Countertop .
:Bowls a psr:Object ;
    dul:hasLocation :Countertop .
:Bottles a psr:Object ;
    dul:hasLocation :Countertop .
:CerealBoxes a psr:Object ;
    dul:hasLocation :Countertop .
:LoafOfBread a psr:Object ;
    dul:hasLocation :Countertop .
:RollOfPaperTowels a psr:Object ;
    dul:hasLocation :DiningTable .
:CartonOfMilk a psr:Object ;
    dul:hasLocation :DiningTable .
:SmallRefrigerator a psr:Appliance ;
    dul:hasLocation :DiningTable .
:Bowl a psr:Object ;
    dul:hasLocation :DiningTable .