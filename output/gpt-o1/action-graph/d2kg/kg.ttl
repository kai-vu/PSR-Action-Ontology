ex:pitcher a psr:Object .
ex:wine_glass a psr:Object .

# 1) Orange mug -> overhead cabinet
ex:a1 a dul:Action ;
    psr:actsOn ex:orange_mug ;
    psr:isAffordedBy soma:PickingUp ;
    soma:isPerformedBy ex:robot1 ;
    psr:precedes ex:a2 .

ex:a2 a dul:Action ;
    psr:actsOn ex:orange_mug, ex:overhead_cabinet ;
    psr:isAffordedBy soma:PuttingDown ;
    soma:isPerformedBy ex:robot1 ;
    psr:precedes ex:a3 .

