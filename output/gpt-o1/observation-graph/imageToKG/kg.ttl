@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix geo:    <http://www.opengis.net/ont/geosparql#> .
@prefix soma:   <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul:    <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr:    <https://w3id.org/psr-task#> .

:KitchenEnv a psr:Environment ;
   dul:hasComponent :Fridge,
                    :KitchenCountertop,
                    :UpperCabinet,
                    :DiningTable,
                    :Chair1, :Chair2, :Chair3, :Chair4,
                    :Pot,
                    :Jam, :Jello, :Salt,
                    :CerealBoxRiceBubbles,
                    :CerealBoxChucky,
                    :PlateStack,
                    :Mug,
                    :BoxOfTea,
                    :FruitBowl,
                    :WaterBottle,
                    :Door,
                    :Window,
                    :RangeHood,
                    :Sink,
                    :Stove .

:Fridge a psr:Appliance ;
   psr:hasAffordance :FridgeOpening,
                     :FridgeClosing .

:KitchenCountertop a psr:Furniture .

:UpperCabinet a psr:Furniture .

:DiningTable a psr:Furniture .

:Chair1 a psr:Furniture .
:Chair2 a psr:Furniture .
:Chair3 a psr:Furniture .
:Chair4 a psr:Furniture .

:Pot a psr:Object ;
   psr:hasAffordance :PotPickingUp,
                      :PotHolding ;
   dul:hasLocation :PotLocation .

:Jam a psr:Object ;
   psr:hasAffordance :JamPickingUp,
                      :JamHolding ;
   dul:hasLocation :JamLocation .

:Jello a psr:Object ;
   psr:hasAffordance :JelloPickingUp,
                      :JelloHolding ;
   dul:hasLocation :JelloLocation .

:Salt a psr:Object ;
   psr:hasAffordance :SaltPickingUp,
                      :SaltHolding ;
   dul:hasLocation :SaltLocation .

:CerealBoxRiceBubbles a psr:Object ;
   psr:hasAffordance :RiceBubblesPickingUp,
                      :RiceBubblesHolding ;
   dul:hasLocation :RiceBubblesLocation .

:CerealBoxChucky a psr:Object ;
   psr:hasAffordance :ChuckyCerealPickingUp,
                      :ChuckyCerealHolding ;
   dul:hasLocation :ChuckyCerealLocation .

:PlateStack a psr:Object ;
   psr:hasAffordance :PlatePickingUp,
                      :PlateHolding ;
   dul:hasLocation :PlateStackLocation .

:Mug a psr:Object ;
   psr:hasAffordance :MugPickingUp,
                      :MugHolding ;
   dul:hasLocation :MugLocation .

:BoxOfTea a psr:Object ;
   psr:hasAffordance :TeaBoxPickingUp,
                      :TeaBoxHolding ;
   dul:hasLocation :TeaBoxLocation .

:FruitBowl a psr:Object ;
   psr:hasAffordance :FruitBowlPickingUp,
                      :FruitBowlHolding ;
   dul:hasLocation :FruitBowlLocation .

:WaterBottle a psr:Object ;
   psr:hasAffordance :BottlePickingUp,
                      :BottleHolding ;
   dul:hasLocation :WaterBottleLocation .

:Door a psr:Object ;
   psr:hasAffordance :DoorOpening,
                      :DoorClosing .

:Window a psr:Object ;
   psr:hasAffordance :WindowOpening,
                      :WindowClosing .

:RangeHood a psr:Appliance .

:Sink a psr:Component .

:Stove a psr:Appliance .

:FridgeOpening a soma:Opening .
:FridgeClosing a soma:Closing .

:PotPickingUp a soma:PickingUp .
:PotHolding a soma:Holding .

:JamPickingUp a soma:PickingUp .
:JamHolding a soma:Holding .

:JelloPickingUp a soma:PickingUp .
:JelloHolding a soma:Holding .

:SaltPickingUp a soma:PickingUp .
:SaltHolding a soma:Holding .

:RiceBubblesPickingUp a soma:PickingUp .
:RiceBubblesHolding a soma:Holding .

:ChuckyCerealPickingUp a soma:PickingUp .
:ChuckyCerealHolding a soma:Holding .

:PlatePickingUp a soma:PickingUp .
:PlateHolding a soma:Holding .

:MugPickingUp a soma:PickingUp .
:MugHolding a soma:Holding .

:TeaBoxPickingUp a soma:PickingUp .
:TeaBoxHolding a soma:Holding .

:FruitBowlPickingUp a soma:PickingUp .
:FruitBowlHolding a soma:Holding .

:BottlePickingUp a soma:PickingUp .
:BottleHolding a soma:Holding .

:DoorOpening a soma:Opening .
:DoorClosing a soma:Closing .

:WindowOpening a soma:Opening .
:WindowClosing a soma:Closing .

:PotLocation a psr:CurrentLocation ;
   psr:onTopOf :Stove .

:JamLocation a psr:CurrentLocation ;
   psr:onTopOf :KitchenCountertop .

:JelloLocation a psr:CurrentLocation ;
   psr:onTopOf :KitchenCountertop .

:SaltLocation a psr:CurrentLocation ;
   psr:onTopOf :KitchenCountertop .

:RiceBubblesLocation a psr:CurrentLocation ;
   psr:onTopOf :KitchenCountertop .

:ChuckyCerealLocation a psr:CurrentLocation ;
   psr:onTopOf :KitchenCountertop .

:PlateStackLocation a psr:CurrentLocation ;
   psr:onTopOf :KitchenCountertop .

:MugLocation a psr:CurrentLocation ;
   psr:onTopOf :KitchenCountertop .

:TeaBoxLocation a psr:CurrentLocation ;
   psr:onTopOf :DiningTable .

:FruitBowlLocation a psr:CurrentLocation ;
   psr:onTopOf :DiningTable .

:WaterBottleLocation a psr:CurrentLocation ;
   psr:onTopOf :DiningTable .