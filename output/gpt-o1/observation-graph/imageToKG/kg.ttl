@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix geo:    <http://www.opengis.net/ont/geosparql#> .
@prefix soma:   <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul:    <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr:    <https://w3id.org/psr-task#> .
@prefix :       <http://example.org/kitchen#> .

:KitchenEnvironment a psr:Environment ;
    dul:hasComponent :Fridge,
                     :UpperCabinet,
                     :LowerCabinet,
                     :DiningTable,
                     :DiningChair1,
                     :DiningChair2,
                     :DiningChair3,
                     :DiningChair4,
                     :Stove,
                     :Sink,
                     :Window,
                     :Hood,
                     :CountertopLocation,
                     :StovetopLocation,
                     :TabletopLocation,
                     :Jam,
                     :JelloBox,
                     :SaltBox,
                     :CerealBox1,
                     :CerealBox2,
                     :Plates,
                     :Pot,
                     :Mug,
                     :WaterBottle,
                     :FruitBowl,
                     :Glass,
                     :Pitcher
.

:Fridge a psr:Appliance ;
    psr:hasAffordance soma:Opening,
                       soma:Closing
.

:UpperCabinet a psr:Furniture ;
    psr:hasAffordance soma:Opening,
                       soma:Closing
.

:LowerCabinet a psr:Furniture .

:DiningTable a psr:Furniture .

:DiningChair1 a psr:Furniture .
:DiningChair2 a psr:Furniture .
:DiningChair3 a psr:Furniture .
:DiningChair4 a psr:Furniture .

:Stove a psr:Appliance .

:Sink a psr:Appliance .

:Window a psr:Object ;
    psr:hasAffordance soma:Opening,
                       soma:Closing
.

:Hood a psr:Appliance .

:CountertopLocation a psr:CurrentLocation ;
    psr:onTopOf :LowerCabinet
.

:StovetopLocation a psr:CurrentLocation ;
    psr:onTopOf :Stove
.

:TabletopLocation a psr:CurrentLocation ;
    psr:onTopOf :DiningTable
.

:Jam a psr:Object ;
    dul:hasLocation :CountertopLocation ;
    psr:hasAffordance soma:Opening
.

:JelloBox a psr:Object ;
    dul:hasLocation :CountertopLocation ;
    psr:hasAffordance soma:Opening
.

:SaltBox a psr:Object ;
    dul:hasLocation :CountertopLocation ;
    psr:hasAffordance soma:Opening
.

:CerealBox1 a psr:Object ;
    dul:hasLocation :CountertopLocation ;
    psr:hasAffordance soma:Opening
.

:CerealBox2 a psr:Object ;
    dul:hasLocation :CountertopLocation ;
    psr:hasAffordance soma:Opening
.

:Plates a psr:Object ;
    dul:hasLocation :CountertopLocation ;
    psr:hasAffordance soma:Holding
.

:Pot a psr:Object ;
    dul:hasLocation :StovetopLocation ;
    psr:hasAffordance soma:Holding
.

:Mug a psr:Object ;
    dul:hasLocation :CountertopLocation ;
    psr:hasAffordance soma:Holding
.

:WaterBottle a psr:Object ;
    dul:hasLocation :TabletopLocation ;
    psr:hasAffordance soma:Holding
.

:FruitBowl a psr:Object ;
    dul:hasLocation :TabletopLocation ;
    psr:hasAffordance soma:Holding
.

:Glass a psr:Object ;
    dul:hasLocation :TabletopLocation ;
    psr:hasAffordance soma:Holding
.

:Pitcher a psr:Object ;
    dul:hasLocation :TabletopLocation ;
    psr:hasAffordance soma:Holding
.