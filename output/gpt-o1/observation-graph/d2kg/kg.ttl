@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/psr-task#> .
@prefix ex: <http://example.org/> .

ex:KitchenDiningArea a psr:Environment ;
    dul:hasComponent ex:DarkCabinets,
                     ex:WhiteRefrigerator,
                     ex:BeigeCountertop,
                     ex:UpperBlackCabinets,
                     ex:JarOfStrawberryJam,
                     ex:BoxJello,
                     ex:ContainerAlpenSalz,
                     ex:ShortStackWhitePlates,
                     ex:BoxRiceBubbles,
                     ex:BoxChuckyMania,
                     ex:CornerCooktop,
                     ex:SmallMetalPot,
                     ex:SingleMug,
                     ex:WoodenRangeHood,
                     ex:StainlessSteelSink,
                     ex:WoodenFramedWindow,
                     ex:RectangularWoodenTable,
                     ex:WhiteHighBackedChairs,
                     ex:LargeEvianWaterBottle,
                     ex:WhiteFruitBowl,
                     ex:RoundFruits,
                     ex:BlackRectangularBoxShortbread,
                     ex:ClearGlassPitcher,
                     ex:SingleWineGlass,
                     ex:WhiteDoor,
                     ex:ParquetWoodFloor .

ex:DarkCabinets a psr:Furniture .
ex:WhiteRefrigerator a psr:Appliance .
ex:BeigeCountertop a psr:Furniture .
ex:UpperBlackCabinets a psr:Furniture .
ex:CornerCooktop a psr:Appliance .
ex:WoodenRangeHood a psr:Appliance .
ex:StainlessSteelSink a psr:Appliance .
ex:WoodenFramedWindow a psr:Component .
ex:RectangularWoodenTable a psr:Furniture .
ex:WhiteHighBackedChairs a psr:Furniture .
ex:WhiteDoor a psr:Component .
ex:ParquetWoodFloor a psr:Component .

ex:JarOfStrawberryJam a psr:Object ;
    dul:hasLocation ex:JarOfStrawberryJam_LOC .

ex:JarOfStrawberryJam_LOC a psr:CurrentLocation ;
    psr:onTopOf ex:BeigeCountertop .

ex:BoxJello a psr:Object ;
    dul:hasLocation ex:BoxJello_LOC .

ex:BoxJello_LOC a psr:CurrentLocation ;
    psr:onTopOf ex:BeigeCountertop .

ex:ContainerAlpenSalz a psr:Object ;
    dul:hasLocation ex:ContainerAlpenSalz_LOC .

ex:ContainerAlpenSalz_LOC a psr:CurrentLocation ;
    psr:onTopOf ex:BeigeCountertop .

ex:ShortStackWhitePlates a psr:Object ;
    dul:hasLocation ex:ShortStackWhitePlates_LOC .

ex:ShortStackWhitePlates_LOC a psr:CurrentLocation ;
    psr:onTopOf ex:BeigeCountertop .

ex:BoxRiceBubbles a psr:Object ;
    dul:hasLocation ex:BoxRiceBubbles_LOC .

ex:BoxRiceBubbles_LOC a psr:CurrentLocation ;
    psr:onTopOf ex:BeigeCountertop .

ex:BoxChuckyMania a psr:Object ;
    dul:hasLocation ex:BoxChuckyMania_LOC .

ex:BoxChuckyMania_LOC a psr:CurrentLocation ;
    psr:onTopOf ex:BeigeCountertop .

ex:SmallMetalPot a psr:Object ;
    dul:hasLocation ex:SmallMetalPot_LOC .

ex:SmallMetalPot_LOC a psr:CurrentLocation ;
    psr:onTopOf ex:BeigeCountertop .

ex:SingleMug a psr:Object ;
    dul:hasLocation ex:SingleMug_LOC .

ex:SingleMug_LOC a psr:CurrentLocation ;
    psr:onTopOf ex:BeigeCountertop .

ex:LargeEvianWaterBottle a psr:Object ;
    dul:hasLocation ex:LargeEvianWaterBottle_LOC .

ex:LargeEvianWaterBottle_LOC a psr:CurrentLocation ;
    geo:sfWithin ex:WhiteFruitBowl .

ex:WhiteFruitBowl a psr:Object ;
    dul:hasLocation ex:WhiteFruitBowl_LOC .

ex:WhiteFruitBowl_LOC a psr:CurrentLocation ;
    psr:onTopOf ex:RectangularWoodenTable .

ex:RoundFruits a psr:Object ;
    dul:hasLocation ex:RoundFruits_LOC .

ex:RoundFruits_LOC a psr:CurrentLocation ;
    geo:sfWithin ex:WhiteFruitBowl .

ex:BlackRectangularBoxShortbread a psr:Object ;
    dul:hasLocation ex:BlackRectangularBoxShortbread_LOC .

ex:BlackRectangularBoxShortbread_LOC a psr:CurrentLocation ;
    psr:onTopOf ex:RectangularWoodenTable .

ex:ClearGlassPitcher a psr:Object ;
    dul:hasLocation ex:ClearGlassPitcher_LOC .

ex:ClearGlassPitcher_LOC a psr:CurrentLocation ;
    psr:onTopOf ex:RectangularWoodenTable .

ex:SingleWineGlass a psr:Object ;
    dul:hasLocation ex:SingleWineGlass_LOC .

ex:SingleWineGlass_LOC a psr:CurrentLocation ;
    psr:onTopOf ex:RectangularWoodenTable .