@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix ros:    <http://example.org/ontoRos#>.
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .


<http://example.org/ontoRos#>   a   owl:Ontology ;
    rdfs:label                      "OntoROS ontology" ;
    owl:versionInfo                 "0.1" .


ros:KitchenEnvironment  a   owl:Class ;
    rdfs:label              "Kitchen Environment" .

ros:Object              a   owl:Class ;
    rdfs:label              "Kitchen Object" ;
    rdfs:comment            "representing the moovable objects in the kitchen e.g. milk carton, cereal box etc.." .

ros:Component           a   owl:Class ;
    rdfs:label              "Kitchen Component" ;
    rdfs:comment            "representing the components making the kitchen e.g. fridge, dishwasher, cabinets etc.." .

ros:ObjectsStack        a   owl:Class ;
    rdfs:label              "Objects Group" ;
    rdfs:comment            "representing a group of objects that are stacked" .

ros:hasObject           a   owl:ObjectProperty ;
    rdfs:label              "has object" ;
    rdfs:domain             ros:KitchenEnvironment ;
    rdfs:range              ros:Object .

ros:hasComponent        a   owl:ObjectProperty ;
    rdfs:label              "has component" ;
    rdfs:domain             ros:KitchenEnvironment ;
    rdfs:range              ros:Component .

ros:hasCurrentLocation  a   owl:ObjectProperty ;
    rdfs:label              "hasCurrentLocation" ;
    rdfs:domain             ros:Object ;
    rdfs:range              ros:Component .

ros:hasTargetLocation   a   owl:ObjectProperty ;
    rdfs:label              "hasCurrentLocation" ;
    rdfs:domain             ros:Object ;
    rdfs:range              ros:Component .

ros:hasStackObject      a   owl:ObjectProperty ;
    rdfs:label              "has stack object" ;
    rdfs:domain             ros:ObjectsStack ;
    rdfs:range              ros:Object .

ros:hasOrderInStack     a   owl:DataProperty ;
    rdfs:label              "has order in stack" ;
    rdfs:domain             ros:Object ;
    rdfs:range              xsd:int .

